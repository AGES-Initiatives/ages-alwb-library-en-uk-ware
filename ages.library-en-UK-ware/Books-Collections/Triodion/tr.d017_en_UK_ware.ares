trVE.AposIdiomelon1.mode = misc_en_UK_ware.Mode8
trVE.AposIdiomelon1.melody = misc_en_UK_ware.Idiomelon
trVE.AposIdiomelon1.incipit = "Let us make haste to…"
trVE.AposIdiomelon1.text = "Let us make haste to wash away through fasting the filth of our transgressions, and through acts of mercy and compassion to the needy let us enter into the bridal chamber of the Bridegroom Christ, who grants us great mercy."

trVE.AposMartyric.mode = misc_en_UK_ware.Mode8
trVE.AposMartyric.melody = ""
trVE.AposMartyric.incipit = "O martyrs of the Lord…"
trVE.AposMartyric.text = "O martyrs of the Lord, we beseech you, offer intercession to our God: pray for abundant mercy on our souls and the forgiveness of our many sins." //oc.m8.d3_en_UK_ware.ocVE.Aposticha3.text

trVE.AposTheotokion.mode = misc_en_UK_ware.Mode8
trVE.AposTheotokion.melody = ""
trVE.AposTheotokion.incipit = "Let us honour the most…"
trVE.AposTheotokion.text = "Let us honour the most pure and glorious Virgin, the true Theotokos, and with right faith let us sing her praises as the Mother of God. Let us greet her as the Angel did: Hail! holy Mother of the incarnate Son of God; Hail! habitation of the Holy Spirit, who dost ever intercede on our behalf for the salvation of our souls."

trMA.Kathisma21.mode = misc_en_UK_ware.Mode8
trMA.Kathisma21.melody = he.a.m8_en_UK_ware.TinSofianKaiLogon.name
trMA.Kathisma21.incipit = "O Word supreme in love…"
trMA.Kathisma21.text = "O Word supreme in love, who with the Father and the Spirit hast created all things visible and invisible in Thy wisdom past speech, grant in Thy compassion that we may spend the season of the joyful Fast in profound peace. Destroy the beguilement of bitter sin, granting us contrition, tears of healing, and forgiveness of our trespasses, that, fasting with a fervent spirit and undoubting soul, we may join the angels to sing the praises of Thy power."

trMA.Kathisma22.mode = misc_en_UK_ware.Mode8
trMA.Kathisma22.melody = misc_en_UK_ware.AutomelonNotMetered
trMA.Kathisma22.incipit = he.a.m8_en_UK_ware.TinSofianKaiLogon.incipit
trMA.Kathisma22.text = he.a.m8_en_UK_ware.TinSofianKaiLogon.text

trMA.C1.poet = poets_en_UK_ware.KyriouIosif

trMA.C2.poet = poets_en_UK_ware.KyriouIosif

trMA.Ode2C1H.ode = misc_en_UK_ware.Ode2
trMA.Ode2C1H.mode = misc_en_UK_ware.Mode3
trMA.Ode2C1H.melody = he.h.m3_en_UK_ware.OsOmvrosEpAgrostin.melody
trMA.Ode2C1H.incipit = he.h.m3_en_UK_ware.OsOmvrosEpAgrostin.incipit
trMA.Ode2C1H.text = he.h.m3_en_UK_ware.OsOmvrosEpAgrostin.text

trMA.Ode2C11.ode = misc_en_UK_ware.Ode2
trMA.Ode2C11.mode = misc_en_UK_ware.Mode3
trMA.Ode2C11.melody = he.h.m3_en_UK_ware.OsOmvrosEpAgrostin.name
trMA.Ode2C11.incipit = "The preparation for the Fast…"
trMA.Ode2C11.text = "The preparation for the Fast shines upon us as the dawn with the light of repentance."

trMA.Ode2C12.ode = misc_en_UK_ware.Ode2
trMA.Ode2C12.mode = misc_en_UK_ware.Mode3
trMA.Ode2C12.melody = he.h.m3_en_UK_ware.OsOmvrosEpAgrostin.name
trMA.Ode2C12.incipit = "O Christ, in the abundance…"
trMA.Ode2C12.text = "O Christ, in the abundance of Thy compassion deliver me from the outer darkness, for Thou alone art merciful."

trMA.Ode2C13.ode = misc_en_UK_ware.Ode2
trMA.Ode2C13.mode = misc_en_UK_ware.Mode3
trMA.Ode2C13.melody = he.h.m3_en_UK_ware.OsOmvrosEpAgrostin.name
trMA.Ode2C13.incipit = "Cleanse me by the grace…"
trMA.Ode2C13.text = "Cleanse me by the grace of repentance, O Saviour, and snatch me from the fire and the worm that never sleeps."

trMA.Ode2C1T.ode = misc_en_UK_ware.Ode2
trMA.Ode2C1T.mode = misc_en_UK_ware.Mode3
trMA.Ode2C1T.melody = he.h.m3_en_UK_ware.OsOmvrosEpAgrostin.name
trMA.Ode2C1T.incipit = "O Virgin worthy of all…"
trMA.Ode2C1T.text = "O Virgin worthy of all praise, be to us a path of repentance, leading us to the gates of salvation."

trMA.Ode2C21.ode = misc_en_UK_ware.Ode2
trMA.Ode2C21.mode = misc_en_UK_ware.Mode2
trMA.Ode2C21.melody = he.h.m2_en_UK_ware.ProsecheteLaosMou.name
trMA.Ode2C21.incipit = "The season of repentance is…"
trMA.Ode2C21.text = "The season of repentance is at hand; in our love for Christ let us hasten to be cleansed of all transgressions, that when we appear before the Master we may be numbered with the blessed."

trMA.Ode2C22.ode = misc_en_UK_ware.Ode2
trMA.Ode2C22.mode = misc_en_UK_ware.Mode2
trMA.Ode2C22.melody = he.h.m2_en_UK_ware.ProsecheteLaosMou.name
trMA.Ode2C22.incipit = "Let us hasten now and…"
trMA.Ode2C22.text = "Let us hasten now and keep a fast that is constant, pure and sinless, that so we may receive remission of our trespasses."

trMA.Ode2C23.ode = misc_en_UK_ware.Ode2
trMA.Ode2C23.mode = misc_en_UK_ware.Mode2
trMA.Ode2C23.melody = he.h.m2_en_UK_ware.ProsecheteLaosMou.name
trMA.Ode2C23.incipit = "O Christ, Thou art loving…"
trMA.Ode2C23.text = "O Christ, Thou art loving and abundant in mercy, always accepting everyone in repentance; therefore we all glorify Thee for ever."

trMA.Ode2C2Tr.ode = misc_en_UK_ware.Ode2
trMA.Ode2C2Tr.mode = misc_en_UK_ware.Mode2
trMA.Ode2C2Tr.melody = he.h.m2_en_UK_ware.ProsecheteLaosMou.name
trMA.Ode2C2Tr.incipit = "Let us glorify the Trinity…"
trMA.Ode2C2Tr.text = "Let us glorify the Trinity in Unity, inseparable, unconfused, God undivided in Substance yet distinct in Persons."

trMA.Ode2C2T.ode = misc_en_UK_ware.Ode2
trMA.Ode2C2T.mode = misc_en_UK_ware.Mode2
trMA.Ode2C2T.melody = he.h.m2_en_UK_ware.ProsecheteLaosMou.name
trMA.Ode2C2T.incipit = "O pure Virgin Mother, from…"
trMA.Ode2C2T.text = "O pure Virgin Mother, from whom the Word took human flesh, entreat Him to save thy servants from peril."

trMA.Ode2C2H.ode = misc_en_UK_ware.Ode2
trMA.Ode2C2H.mode = misc_en_UK_ware.Mode2
trMA.Ode2C2H.melody = he.h.m2_en_UK_ware.ProsecheteLaosMou.melody
trMA.Ode2C2H.incipit = he.h.m2_en_UK_ware.ProsecheteLaosMou.incipit
trMA.Ode2C2H.text = he.h.m2_en_UK_ware.ProsecheteLaosMou.text

trMA.Ode8C11.ode = misc_en_UK_ware.Ode8
trMA.Ode8C11.mode = misc_en_UK_ware.Mode3
trMA.Ode8C11.melody = he.h.m3_en_UK_ware.TonExAnarchou.name
trMA.Ode8C11.incipit = "Now is the time for…"
trMA.Ode8C11.text = "Now is the time for turning back: O my soul, hasten to show unto God worthy fruits of repentance. Keeping fast and praying, rise up and cry urgently: Praise, bless and exalt Christ above all for ever."

trMA.Ode8C12.ode = misc_en_UK_ware.Ode8
trMA.Ode8C12.mode = misc_en_UK_ware.Mode3
trMA.Ode8C12.melody = he.h.m3_en_UK_ware.TonExAnarchou.name
trMA.Ode8C12.incipit = "O brethren, let us cleanse…"
trMA.Ode8C12.text = "O brethren, let us cleanse the senses of our soul, and cleave to the Lord with contrition; and let us cry aloud sincerely, in purity and repentance: Praise, bless and exalt Christ above all for ever."

trMA.Ode8C13.ode = misc_en_UK_ware.Ode8
trMA.Ode8C13.mode = misc_en_UK_ware.Mode3
trMA.Ode8C13.melody = he.h.m3_en_UK_ware.TonExAnarchou.name
trMA.Ode8C13.incipit = "With persistent prayer let us…"
trMA.Ode8C13.text = "With persistent prayer let us make haste to slay the enemy who lies in wait for us with thoughts of gluttony, and beguiles our senses with uncontrolled desire for swiftly-passing pleasure: and let us praise, bless and exalt Christ above all for ever."

trMA.Ode8C1T.ode = misc_en_UK_ware.Ode8
trMA.Ode8C1T.mode = misc_en_UK_ware.Mode3
trMA.Ode8C1T.melody = he.h.m3_en_UK_ware.TonExAnarchou.name
trMA.Ode8C1T.incipit = "O Virgin, thou hast made…"
trMA.Ode8C1T.text = "O Virgin, thou hast made our earthly nature heavenly: with the rain of thy merciful compassion make the barren furrow of my earthbound soul fruitful in virtues, and I will praise and exalt Christ above all for ever."

trMA.Ode8C21.ode = misc_en_UK_ware.Ode8
trMA.Ode8C21.mode = misc_en_UK_ware.Mode2
trMA.Ode8C21.melody = he.h.m2_en_UK_ware.TonEnTiVatoMosi.name
trMA.Ode8C21.incipit = "O ye faithful, let us…"
trMA.Ode8C21.text = "O ye faithful, let us now in preparation cleanse ourselves not only in our eating but in our actions, and let us begin the Fast with fervent minds, that so we may appear before the Creator in glory for ever."

trMA.Ode8C22.ode = misc_en_UK_ware.Ode8
trMA.Ode8C22.mode = misc_en_UK_ware.Mode2
trMA.Ode8C22.melody = he.h.m2_en_UK_ware.TonEnTiVatoMosi.name
trMA.Ode8C22.incipit = "This should be the manner…"
trMA.Ode8C22.text = "This should be the manner of our fasting: not in hatred and contention, not in envy and strife, not in self-glory and with hidden deceit, but like Christ in humblemindedness."

trMA.Ode8C23.ode = misc_en_UK_ware.Ode8
trMA.Ode8C23.mode = misc_en_UK_ware.Mode2
trMA.Ode8C23.melody = he.h.m2_en_UK_ware.TonEnTiVatoMosi.name
trMA.Ode8C23.incipit = "Those who show mercy to…"
trMA.Ode8C23.text = "Those who show mercy to the poor, so it is said, lend their money wisely to the Saviour. O joy beyond compare! For in return He grants rich blessings unto all eternity."

trMA.Ode8C2Tr.ode = misc_en_UK_ware.Ode8
trMA.Ode8C2Tr.mode = misc_en_UK_ware.Mode2
trMA.Ode8C2Tr.melody = he.h.m2_en_UK_ware.TonEnTiVatoMosi.name
trMA.Ode8C2Tr.incipit = "Glory to the Father, Son…"
trMA.Ode8C2Tr.text = "Glory to the Father, Son and Holy Spirit, God one in three. We mortals praise Thee with the angels, O Most Holy Trinity, unceasingly for ever."

trMA.Ode8C2T.ode = misc_en_UK_ware.Ode8
trMA.Ode8C2T.mode = misc_en_UK_ware.Mode2
trMA.Ode8C2T.melody = he.h.m2_en_UK_ware.TonEnTiVatoMosi.name
trMA.Ode8C2T.incipit = "Hail, pure Virgin, Theotokos Mary…"
trMA.Ode8C2T.text = "Hail, pure Virgin, Theotokos Mary, mercy-seat of the world, strength of Orthodox Christians: pray that we may be delivered from the fire that never shall be quenched, as we glorify thee for ever."

trMA.Ode8C2H.ode = misc_en_UK_ware.Ode8
trMA.Ode8C2H.mode = misc_en_UK_ware.Mode2
trMA.Ode8C2H.melody = he.h.m2_en_UK_ware.TonEnTiVatoMosi.melody
trMA.Ode8C2H.incipit = he.h.m2_en_UK_ware.TonEnTiVatoMosi.incipit
trMA.Ode8C2H.text = he.h.m2_en_UK_ware.TonEnTiVatoMosi.text

trMA.Ode9C11.ode = misc_en_UK_ware.Ode9
trMA.Ode9C11.mode = misc_en_UK_ware.Mode3
trMA.Ode9C11.melody = he.h.m3_en_UK_ware.SeTinAkataflekton.name
trMA.Ode9C11.incipit = "Show unto us Thy pure…"
trMA.Ode9C11.text = "Show unto us Thy pure and loving compassion, for Thou dost grant repentance to mortal men for their salvation, O Saviour who alone art rich in mercy."

trMA.Ode9C12.ode = misc_en_UK_ware.Ode9
trMA.Ode9C12.mode = misc_en_UK_ware.Mode3
trMA.Ode9C12.melody = he.h.m3_en_UK_ware.SeTinAkataflekton.name
trMA.Ode9C12.incipit = "Woe is me! How fearful…"
trMA.Ode9C12.text = "Woe is me! How fearful is that judgement seat where there is no respect of persons; and we shall stand before it stripped, rendering an account of all the evil actions in our life."

trMA.Ode9C13.ode = misc_en_UK_ware.Ode9
trMA.Ode9C13.mode = misc_en_UK_ware.Mode3
trMA.Ode9C13.melody = he.h.m3_en_UK_ware.SeTinAkataflekton.name
trMA.Ode9C13.incipit = "Rain down upon us, O…"
trMA.Ode9C13.text = "Rain down upon us, O Benefactor, the waters of compassion, drowning all our filth, and grant us opportunity for repentance."

trMA.Ode9C14.ode = misc_en_UK_ware.Ode9
trMA.Ode9C14.mode = misc_en_UK_ware.Mode3
trMA.Ode9C14.melody = he.h.m3_en_UK_ware.SeTinAkataflekton.name
trMA.Ode9C14.incipit = "In joy, with eager hearts…"
trMA.Ode9C14.text = "In joy, with eager hearts, together let us all enter the forecourt of the holy Fast and offer hymns of thanksgiving to Christ."

trMA.Ode9C1T.ode = misc_en_UK_ware.Ode9
trMA.Ode9C1T.mode = misc_en_UK_ware.Mode3
trMA.Ode9C1T.melody = he.h.m3_en_UK_ware.SeTinAkataflekton.name
trMA.Ode9C1T.incipit = "Singing thy praises with true…"
trMA.Ode9C1T.text = "Singing thy praises with true faith, we entreat thee, our firm protection and strong hope: O Virgin, intercede with thy Son for our salvation."

trMA.Ode9C21.ode = misc_en_UK_ware.Ode9
trMA.Ode9C21.mode = misc_en_UK_ware.Mode2
trMA.Ode9C21.melody = he.h.m2_en_UK_ware.TinParthenianSou.name
trMA.Ode9C21.incipit = "Let us now in preparation…"
trMA.Ode9C21.text = "Let us now in preparation cleanse ourselves through tears and fasting and great acts of virtue, that we may receive great mercy."

trMA.Ode9C22.ode = misc_en_UK_ware.Ode9
trMA.Ode9C22.mode = misc_en_UK_ware.Mode2
trMA.Ode9C22.melody = he.h.m2_en_UK_ware.TinParthenianSou.name
trMA.Ode9C22.incipit = "Let us now follow the…"
trMA.Ode9C22.text = "Let us now follow the example of the wise virgins, and in our love for Christ let us hasten to meet Him with brightly burning lamps."

trMA.Ode9C23.ode = misc_en_UK_ware.Ode9
trMA.Ode9C23.mode = misc_en_UK_ware.Mode2
trMA.Ode9C23.melody = he.h.m2_en_UK_ware.TinParthenianSou.name
trMA.Ode9C23.incipit = "Let us now follow the…"
trMA.Ode9C23.text = "Let us now follow the example of Elijah who fasted and fled from wicked Jezebel, that like him we may be taken up from the earth."

trMA.Ode9C2Tr.ode = misc_en_UK_ware.Ode9
trMA.Ode9C2Tr.mode = misc_en_UK_ware.Mode2
trMA.Ode9C2Tr.melody = he.h.m2_en_UK_ware.TinParthenianSou.name
trMA.Ode9C2Tr.incipit = "Holy, Holy, Holy, thrice holy…"
trMA.Ode9C2Tr.text = "Holy, Holy, Holy, thrice holy art Thou, Father almighty and consubstantial Son with the Spirit, one Godhead."

trMA.Ode9C2T.ode = misc_en_UK_ware.Ode9
trMA.Ode9C2T.mode = misc_en_UK_ware.Mode2
trMA.Ode9C2T.melody = he.h.m2_en_UK_ware.TinParthenianSou.name
trMA.Ode9C2T.incipit = "Thou art our glory, and…"
trMA.Ode9C2T.text = "Thou art our glory, and to thee we run for refuge, O pure and venerated Ever-Virgin: deliver us from torment."

trMA.Ode9C2H.ode = misc_en_UK_ware.Ode9
trMA.Ode9C2H.mode = misc_en_UK_ware.Mode2
trMA.Ode9C2H.melody = he.h.m2_en_UK_ware.TinParthenianSou.melody
trMA.Ode9C2H.incipit = he.h.m2_en_UK_ware.TinParthenianSou.incipit
trMA.Ode9C2H.text = he.h.m2_en_UK_ware.TinParthenianSou.text

trMA.AposIdiomelon1.mode = misc_en_UK_ware.Mode3
trMA.AposIdiomelon1.melody = misc_en_UK_ware.Idiomelon
trMA.AposIdiomelon1.incipit = "O peoples, let us greet…"
trMA.AposIdiomelon1.text = "O peoples, let us greet the Fast with joy, for the beginning of the spiritual contest is at hand. Let us lay aside the comforts of the flesh; let us make God's gifts of grace increase within our soul; let us suffer with Christ as His servants, that we may be also glorified with Him as children of God. And may the Holy Spirit, dwelling in us all, give light to our souls."

trMA.AposMartyric.mode = misc_en_UK_ware.Mode3
trMA.AposMartyric.melody = ""
trMA.AposMartyric.incipit = "The soldiers of Christ cast…"
trMA.AposMartyric.text = "The soldiers of Christ cast aside the fear of kings and tyrants; boldly and with courage they confessed Him as the Lord of all, their King and God: and now they intercede for our souls." //oc.m3.d3_en_UK_ware.ocMA.Aposticha3.text

trMA.AposTheotokion.mode = misc_en_UK_ware.Mode3
trMA.AposTheotokion.melody = ""
trMA.AposTheotokion.incipit = "O Theotokos, the protection of…"
trMA.AposTheotokion.text = "O Theotokos, the protection of all who ask thy prayers, in thee we trust, in thee we boast, in thee is all our hope: pray to thy Son for thine unprofitable servants." //oc.m3.d3_en_UK_ware.ocVE.AposTheotokion.text
