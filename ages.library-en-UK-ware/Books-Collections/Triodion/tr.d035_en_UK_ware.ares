trVE.AposIdiomelon1.mode = miscellanea_en_UK_ware.misc.Mode4
trVE.AposIdiomelon1.melody = ""
trVE.AposIdiomelon1.incipit = "Now is the accepted time…"
trVE.AposIdiomelon1.text = "Now is the accepted time, now is the day of salvation. In the multitude of Thy mercy visit my soul, and release me from the burden of my transgressions, for Thou alone lovest mankind."

trVE.Apolytikion1.mode = miscellanea_en_UK_ware.misc.Mode2
trVE.Apolytikion1.melody = ""
trVE.Apolytikion1.incipit = "Apostles, martyrs and prophets, hierarchs…"
trVE.Apolytikion1.text = "Apostles, martyrs and prophets, hierarchs, holy monks and righteous men, who have fought the good fight to the end and kept the faith: since ye have boldness in the presence of the Saviour, we entreat you, pray to Him on our behalf for the salvation of our souls." //da.d7_en_UK_ware.daVE.Apolytikion1.text

trVE.Apolytikion2.mode = miscellanea_en_UK_ware.misc.Mode2
trVE.Apolytikion2.melody = ""
trVE.Apolytikion2.incipit = "Remember Thy servants, O Lord…"
trVE.Apolytikion2.text = "Remember Thy servants, O Lord, in Thy love, and forgive them all the sins that they committed in this life, for none is without sin but Thee, who hast power to give rest to the departed." //da.d7_en_UK_ware.daVE.Apolytikion2.text

trVE.ApolTheotokion.mode = miscellanea_en_UK_ware.misc.Mode2
trVE.ApolTheotokion.melody = ""
trVE.ApolTheotokion.incipit = "O holy Mother of the…"
trVE.ApolTheotokion.text = "O holy Mother of the ineffable Light, with angelic hymns we honour and magnify thee." //da.d7_en_UK_ware.daVE.ApolTheotokion.text

