trMA.Ode4C2H.incipit = he.h.m6_en_UK_ware.heHE.AkikoenOProfitis.incipit
trMA.Ode4C2H.text = he.h.m6_en_UK_ware.heHE.AkikoenOProfitis.text

trMA.Ode8C11.ode = miscellanea_en_UK_ware.misc.Ode8
trMA.Ode8C11.mode = miscellanea_en_UK_ware.misc.Mode6
trMA.Ode8C11.greekmelody = he.h.m6_en_UK_ware.heHE.EkFlogosToisOsiois.greekname
trMA.Ode8C11.melody = he.h.m6_en_UK_ware.heHE.EkFlogosToisOsiois.name
trMA.Ode8C11.incipit = "Through fasting Moses was granted…"
trMA.Ode8C11.text = "Through fasting Moses was granted the vision of God and Elijah was taken up in a chariot of fire. O my soul, make haste; abstain from harmful thoughts, that thou mayest be delivered from the beguilement that draws thee to the earth."

trMA.Ode8C12.ode = miscellanea_en_UK_ware.misc.Ode8
trMA.Ode8C12.mode = miscellanea_en_UK_ware.misc.Mode6
trMA.Ode8C12.melody = he.h.m6_en_UK_ware.heHE.EkFlogosToisOsiois.name
trMA.Ode8C12.incipit = "I fall into ever greater…"
trMA.Ode8C12.text = "I fall into ever greater sins and inflict fresh wounds upon myself. At the prayers of Thine apostles, O merciful Christ, heal the stony hardness of my heart."

trMA.Ode8C13.ode = miscellanea_en_UK_ware.misc.Ode8
trMA.Ode8C13.mode = miscellanea_en_UK_ware.misc.Mode6
trMA.Ode8C13.melody = he.h.m6_en_UK_ware.heHE.EkFlogosToisOsiois.name
trMA.Ode8C13.incipit = "O stars that shine in…"
trMA.Ode8C13.text = "O stars that shine in all the world, bright with the splendour of God, scatter the clouds of our passions and grant us to worship at the radiant feast of the Resurrection, praising the Sun of glory."

trMA.Ode8C1T.ode = miscellanea_en_UK_ware.misc.Ode8
trMA.Ode8C1T.mode = miscellanea_en_UK_ware.misc.Mode6
trMA.Ode8C1T.melody = he.h.m6_en_UK_ware.heHE.EkFlogosToisOsiois.name
trMA.Ode8C1T.incipit = "O Blessed of God, from…"
trMA.Ode8C1T.text = "O Blessed of God, from all generations we call thee blessed, as thou thyself hast prophesied. For thou alone hast borne unto mortal men the blessed Word, who ineffably took flesh from thee."

trMA.Ode8C21.ode = miscellanea_en_UK_ware.misc.Ode8
trMA.Ode8C21.mode = miscellanea_en_UK_ware.misc.Mode6
trMA.Ode8C21.greekmelody = he.h.m6_en_UK_ware.heHE.OiOsioiSouPaides.greekname
trMA.Ode8C21.melody = he.h.m6_en_UK_ware.heHE.OiOsioiSouPaides.name
trMA.Ode8C21.incipit = "O apostles of the Lord…"
trMA.Ode8C21.text = "O apostles of the Lord, we are swimming in the deep waters of delusion: catch us with the rod of faith, and we shall bless, praise and exalt the Lord above all for ever."

trMA.Ode8C22.ode = miscellanea_en_UK_ware.misc.Ode8
trMA.Ode8C22.mode = miscellanea_en_UK_ware.misc.Mode6
trMA.Ode8C22.melody = he.h.m6_en_UK_ware.heHE.OiOsioiSouPaides.name
trMA.Ode8C22.incipit = "Great is the power which…"
trMA.Ode8C22.text = "Great is the power which Thou hast shown, O Christ, in Thine apostles. For their shadows and the clothing which they wore brought healing to the sick; and they bless, praise and exalt the Lord above all for ever."

trMA.Ode8C2Tr.ode = miscellanea_en_UK_ware.misc.Ode8
trMA.Ode8C2Tr.mode = miscellanea_en_UK_ware.misc.Mode6
trMA.Ode8C2Tr.melody = he.h.m6_en_UK_ware.heHE.OiOsioiSouPaides.name
trMA.Ode8C2Tr.incipit = "Without beginning are the birth…"
trMA.Ode8C2Tr.text = "Without beginning are the birth of the Son and the procession of the Spirit. I worship the Father who begets, I glorify the Son who is begotten, and I sing the praises of the Holy Spirit who shines forth with the Father and the Son."

trMA.Ode8C2T.ode = miscellanea_en_UK_ware.misc.Ode8
trMA.Ode8C2T.mode = miscellanea_en_UK_ware.misc.Mode6
trMA.Ode8C2T.melody = he.h.m6_en_UK_ware.heHE.OiOsioiSouPaides.name
trMA.Ode8C2T.incipit = "Cease not to pray, O…"
trMA.Ode8C2T.text = "Cease not to pray, O Virgin, unto Him whom thou hast borne in ways past all interpretation, for He loves mankind: that He may save from danger all those who flee to thee for refuge."

trMA.Ode8C2A1.ode = miscellanea_en_UK_ware.misc.Ode8
trMA.Ode8C2A1.mode = miscellanea_en_UK_ware.misc.Mode6
trMA.Ode8C2A1.melody = he.h.m6_en_UK_ware.heHE.OiOsioiSouPaides.name
trMA.Ode8C2A1.incipit = "O holy company of the…"
trMA.Ode8C2A1.text = "O holy company of the twelve apostles, grant me uncondemned to venerate the precious Cross, as I bless, praise and exalt the Lord above all for ever."

trMA.Ode8C2H.ode = miscellanea_en_UK_ware.misc.Ode8
trMA.Ode8C2H.mode = miscellanea_en_UK_ware.misc.Mode6
trMA.Ode8C2H.greekmelody = he.h.m6_en_UK_ware.heHE.OiOsioiSouPaides.greekname
trMA.Ode8C2H.melody = he.h.m6_en_UK_ware.heHE.OiOsioiSouPaides.melody
trMA.Ode8C2H.incipit = he.h.m6_en_UK_ware.heHE.OiOsioiSouPaides.incipit
trMA.Ode8C2H.text = he.h.m6_en_UK_ware.heHE.OiOsioiSouPaides.text

trMA.Ode9C11.ode = miscellanea_en_UK_ware.misc.Ode9
trMA.Ode9C11.mode = miscellanea_en_UK_ware.misc.Mode6
trMA.Ode9C11.greekmelody = he.h.m6_en_UK_ware.heHE.TheonAnthropois.greekname
trMA.Ode9C11.melody = he.h.m6_en_UK_ware.heHE.TheonAnthropois.name
trMA.Ode9C11.incipit = "O God and Lord of…"
trMA.Ode9C11.text = "O God and Lord of many mercies, cleanse the wounds of my heart and heal them with the medicine of repentance. I have sinned, I have sinned against Thee: take pity on me and save me at the prayers of Thine apostles, for Thou art full of mercy."

trMA.Ode9C12.ode = miscellanea_en_UK_ware.misc.Ode9
trMA.Ode9C12.mode = miscellanea_en_UK_ware.misc.Mode6
trMA.Ode9C12.melody = he.h.m6_en_UK_ware.heHE.TheonAnthropois.name
trMA.Ode9C12.incipit = "Foolishly, like the Prodigal, I…"
trMA.Ode9C12.text = "Foolishly, like the Prodigal, I have followed my unruly impulses and have departed far from my true self: I eat the rough husks of pleasure and am deprived of the food of salvation. O Christ, take pity on me and save me."

trMA.Ode9C13.ode = miscellanea_en_UK_ware.misc.Ode9
trMA.Ode9C13.mode = miscellanea_en_UK_ware.misc.Mode6
trMA.Ode9C13.melody = he.h.m6_en_UK_ware.heHE.TheonAnthropois.name
trMA.Ode9C13.incipit = "O my soul, thou hast…"
trMA.Ode9C13.text = "O my soul, thou hast not cleansed thyself from evil nor avoided the lusts that corrupt thee. Why art thou filled, then, with unjustified rejoicing, because thou hast observed the Fast? For such is not the fast the Lord has chosen, who desires our true amendment."

trMA.Ode9C1T.ode = miscellanea_en_UK_ware.misc.Ode9
trMA.Ode9C1T.mode = miscellanea_en_UK_ware.misc.Mode6
trMA.Ode9C1T.melody = he.h.m6_en_UK_ware.heHE.TheonAnthropois.name
trMA.Ode9C1T.incipit = "O Virgin Theotokos, who hast…"
trMA.Ode9C1T.text = "O Virgin Theotokos, who hast borne the Light, guide me with divine light and drive away the darkness from my soul, that I may bless thee: for all generations of men, as thou hast prophesied, shall call thee blessed."

trMA.Ode9C21.ode = miscellanea_en_UK_ware.misc.Ode9
trMA.Ode9C21.mode = miscellanea_en_UK_ware.misc.Mode6
trMA.Ode9C21.greekmelody = he.h.m6_en_UK_ware.heHE.AsporouSyllipseos.greekname
trMA.Ode9C21.melody = he.h.m6_en_UK_ware.heHE.AsporouSyllipseos.name
trMA.Ode9C21.incipit = "Simple in speech but wise…"
trMA.Ode9C21.text = "Simple in speech but wise in knowledge, ye have destroyed the webs of words spun by philosophers, the cunning tapestries of the orators, and the divinations of the stargazers: therefore, O apostles of Christ, ye have been revealed as the only teachers of the whole inhabited earth."

trMA.Ode9C22.ode = miscellanea_en_UK_ware.misc.Ode9
trMA.Ode9C22.mode = miscellanea_en_UK_ware.misc.Mode6
trMA.Ode9C22.melody = he.h.m6_en_UK_ware.heHE.AsporouSyllipseos.name
trMA.Ode9C22.incipit = "Peter speaks, and Plato falls…"
trMA.Ode9C22.text = "Peter speaks, and Plato falls silent; Paul teaches, and Pythagoras is heard no more. The company of the apostles, preaching the mysteries of God, has buried the dead voice of the pagan Greeks and called the world to the worship of Christ."

trMA.Ode9C2Tr.ode = miscellanea_en_UK_ware.misc.Ode9
trMA.Ode9C2Tr.mode = miscellanea_en_UK_ware.misc.Mode6
trMA.Ode9C2Tr.melody = he.h.m6_en_UK_ware.heHE.AsporouSyllipseos.name
trMA.Ode9C2Tr.incipit = "O Father, single only-Begetter of…"
trMA.Ode9C2Tr.text = "O Father, single only-Begetter of the only-Begotten Son; O only Light and Brightness from the one and only Light; and Thou, one and only Holy Spirit from the one God, true Lord from the Lord: O holy Three in One, save me as I tell of Thy divinity."

trMA.Ode9C2T.ode = miscellanea_en_UK_ware.misc.Ode9
trMA.Ode9C2T.mode = miscellanea_en_UK_ware.misc.Mode6
trMA.Ode9C2T.melody = he.h.m6_en_UK_ware.heHE.AsporouSyllipseos.name
trMA.Ode9C2T.incipit = "The marvel of thy childbearing…"
trMA.Ode9C2T.text = "The marvel of thy childbearing fills me with wonder, O all-blameless Lady. How hast thou conceived without seed Him whom none can comprehend? How hast thou remained a Virgin and yet become a Mother? ‘Accept the miracle with faith, and worship the Child that is born, for all that He wills, He has the power to do.’"

trMA.Ode9C2A1.ode = miscellanea_en_UK_ware.misc.Ode9
trMA.Ode9C2A1.mode = miscellanea_en_UK_ware.misc.Mode6
trMA.Ode9C2A1.melody = he.h.m6_en_UK_ware.heHE.AsporouSyllipseos.name
trMA.Ode9C2A1.incipit = "At your prayers, O holy…"
trMA.Ode9C2A1.text = "At your prayers, O holy apostles of Christ, may we all be counted worthy to see and venerate -with pure lips and souls the precious Cross, the all-holy Tree that has borne as flower the salvation of the world."

trMA.Ode9C2H.ode = miscellanea_en_UK_ware.misc.Ode9
trMA.Ode9C2H.mode = miscellanea_en_UK_ware.misc.Mode6
trMA.Ode9C2H.greekmelody = he.h.m6_en_UK_ware.heHE.AsporouSyllipseos.greekname
trMA.Ode9C2H.melody = he.h.m6_en_UK_ware.heHE.AsporouSyllipseos.melody
trMA.Ode9C2H.incipit = he.h.m6_en_UK_ware.heHE.AsporouSyllipseos.incipit
trMA.Ode9C2H.text = he.h.m6_en_UK_ware.heHE.AsporouSyllipseos.text

trMA.AposIdiomelon1.mode = miscellanea_en_UK_ware.misc.Mode6
trMA.AposIdiomelon1.melody = miscellanea_en_UK_ware.misc.Idiomelon
trMA.AposIdiomelon1.incipit = "Like the Prodigal I have…"
trMA.AposIdiomelon1.text = "Like the Prodigal I have fallen from sonship, and as a slave I have eaten with the swine, yet was not satisfied by their food; but now I return to Thee, O compassionate Father. Wrongly I forsook Thee, but rightly have I turned back in repentance, and I cry: I have sinned against heaven and before Thee, O Father; grant to me, returning homeward, Thy great mercy."

trMA.AposMartyric.mode = miscellanea_en_UK_ware.misc.Mode6
trMA.AposMartyric.melody = ""
trMA.AposMartyric.incipit = "Great wonders has our God…"
trMA.AposMartyric.text = "Great wonders has our God done for His saints and His elect. Rejoice and be glad, all ye His servants: He has prepared for you crowns of glory and His Kingdom. We entreat you not to forget us." //oc.m6.d5_en_UK_ware.ocMA.Aposticha3.text

trMA.AposTheotokion.mode = miscellanea_en_UK_ware.misc.Mode6
trMA.AposTheotokion.melody = ""
trMA.AposTheotokion.incipit = "O Theotokos, thou art the…"
trMA.AposTheotokion.text = "O Theotokos, thou art the true Vine, that has put forth the fruit of life. We pray thee, O Lady, intercede together with the apostles and all the saints, that mercy may be granted to our souls." //oc.m6.d5_en_UK_ware.ocVE.AposTheotokion.text

trTE.Troparion.mode = miscellanea_en_UK_ware.misc.Mode7
trTE.Troparion.melody = ""
trTE.Troparion.incipit = "O Thou who lovest man…"
trTE.Troparion.text = "O Thou who lovest man, if Thou shouldest mark iniquities, what hope is there for our salvation? But, Lord, in Thy mercy send down upon Thy people Thy help from on high."
