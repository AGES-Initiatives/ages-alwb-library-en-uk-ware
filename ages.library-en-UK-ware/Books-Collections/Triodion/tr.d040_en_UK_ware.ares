trMA.Ode4C11.ode = miscellanea_en_UK_ware.misc.Ode4
trMA.Ode4C11.mode = miscellanea_en_UK_ware.misc.Mode6
trMA.Ode4C11.greekmelody = he.h.m6_en_UK_ware.heHE.ChristosMouDynamis.greekname
trMA.Ode4C11.melody = he.h.m6_en_UK_ware.heHE.ChristosMouDynamis.name
trMA.Ode4C11.incipit = "O ye faithful, let us…"
trMA.Ode4C11.text = "O ye faithful, let us give ourselves to fasting, tears and prayer, to works of compassion and to a contrite way of life, to upright thoughts and purity, that we may share in glory."

trMA.Ode4C12.ode = miscellanea_en_UK_ware.misc.Ode4
trMA.Ode4C12.mode = miscellanea_en_UK_ware.misc.Mode6
trMA.Ode4C12.melody = he.h.m6_en_UK_ware.heHE.ChristosMouDynamis.name
trMA.Ode4C12.incipit = "O apostles, ye shine with…"
trMA.Ode4C12.text = "O apostles, ye shine with radiance as stars in the firmament of the Holy Church: give light to our hearts through the Spirit of God."

trMA.Ode4C13.ode = miscellanea_en_UK_ware.misc.Ode4
trMA.Ode4C13.mode = miscellanea_en_UK_ware.misc.Mode6
trMA.Ode4C13.melody = he.h.m6_en_UK_ware.heHE.ChristosMouDynamis.name
trMA.Ode4C13.incipit = "As divine coals kindled by…"
trMA.Ode4C13.text = "As divine coals kindled by immaterial fire, O wise apostles of our God, burn up the material passions in our hearts."

trMA.Ode4C1T.ode = miscellanea_en_UK_ware.misc.Ode4
trMA.Ode4C1T.mode = miscellanea_en_UK_ware.misc.Mode6
trMA.Ode4C1T.melody = he.h.m6_en_UK_ware.heHE.ChristosMouDynamis.name
trMA.Ode4C1T.incipit = "O all-blameless Lady, lamp that…"
trMA.Ode4C1T.text = "O all-blameless Lady, lamp that never shall be quenched, bridal chamber full of light, higher than the seraphim, the chariot of the cherubim, set me free from grievous sins and dangers."

trMA.Ode4C21.ode = miscellanea_en_UK_ware.misc.Ode4
trMA.Ode4C21.mode = miscellanea_en_UK_ware.misc.Mode6
trMA.Ode4C21.greekmelody = he.h.m6_en_UK_ware.heHE.AkikoenOProfitis.greekname
trMA.Ode4C21.melody = he.h.m6_en_UK_ware.heHE.AkikoenOProfitis.name
trMA.Ode4C21.incipit = "O apostles of the Saviour…"
trMA.Ode4C21.text = "O apostles of the Saviour, shining torches of God’s revelation, ye have dispersed the night of ignorance: ye fill the Church with brightness and give light to the inhabited earth with the fiery tongues of your divine teaching."

trMA.Ode4C22.ode = miscellanea_en_UK_ware.misc.Ode4
trMA.Ode4C22.mode = miscellanea_en_UK_ware.misc.Mode6
trMA.Ode4C22.melody = he.h.m6_en_UK_ware.heHE.AkikoenOProfitis.name
trMA.Ode4C22.incipit = "O apostles, ye journeyed through…"
trMA.Ode4C22.text = "O apostles, ye journeyed through the world, each to a different country, gathering all men into the one faith; and sharing now in the rewards of heaven, ye rejoice and pray without ceasing to Christ, that we may all be saved."

trMA.Ode4C2Tr.ode = miscellanea_en_UK_ware.misc.Ode4
trMA.Ode4C2Tr.mode = miscellanea_en_UK_ware.misc.Mode6
trMA.Ode4C2Tr.melody = he.h.m4_en_UK_ware.heHE.AkikoenOProfitis.name
trMA.Ode4C2Tr.incipit = "I glorify the Son and…"
trMA.Ode4C2Tr.text = "I glorify the Son and the Spirit who come from the Father as light and ray from the sun: the One begotten as Offspring, the Other proceeding and sent forth; divine and coeternal Trinity, adored by all creation."

trMA.Ode4C2T.ode = miscellanea_en_UK_ware.misc.Ode4
trMA.Ode4C2T.mode = miscellanea_en_UK_ware.misc.Mode6
trMA.Ode4C2T.melody = he.h.m6_en_UK_ware.heHE.AkikoenOProfitis.name
trMA.Ode4C2T.incipit = "O honoured Virgin, who hast…"
trMA.Ode4C2T.text = "O honoured Virgin, who hast given birth yet kept thy purity, thou hast borne both God and man, a single Person with a twofold Nature. This thy miracle, O Virgin Mother, fills every ear and mind with wonder."

trMA.Ode4C2A1.ode = miscellanea_en_UK_ware.misc.Ode4
trMA.Ode4C2A1.mode = miscellanea_en_UK_ware.misc.Mode6
trMA.Ode4C2A1.melody = he.h.m6_en_UK_ware.heHE.AkikoenOProfitis.name
trMA.Ode4C2A1.incipit = "At your holy intercessions, apostles…"
trMA.Ode4C2A1.text = "At your holy intercessions, apostles of the Lord, may we be counted worthy with pure lips and eyes to reverence the lifegiving Cross, that is exalted in the sight of the whole world for all to venerate."

trMA.Ode4C2H.ode = miscellanea_en_UK_ware.misc.Ode4
trMA.Ode4C2H.mode = miscellanea_en_UK_ware.misc.Mode6
trMA.Ode4C2H.greekmelody = he.h.m6_en_UK_ware.heHE.AkikoenOProfitis.greekname
