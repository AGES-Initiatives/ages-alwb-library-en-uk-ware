trMA.Ode6C1H.ode = miscellanea_en_UK_ware.misc.Ode6
trMA.Ode6C1H.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode6C1H.melody = he.h.m8_en_UK_ware.heHE.IlasthitiMoiSotir.melody
trMA.Ode6C1H.incipit = he.h.m8_en_UK_ware.heHE.IlasthitiMoiSotir.incipit
trMA.Ode6C1H.text = he.h.m8_en_UK_ware.heHE.IlasthitiMoiSotir.text

trMA.Ode6C11.ode = miscellanea_en_UK_ware.misc.Ode6
trMA.Ode6C11.mode = miscellanea_en_UK_ware.misc.Mode4
trMA.Ode6C11.melody = he.h.m8_en_UK_ware.heHE.IlasthitiMoiSotir.name
trMA.Ode6C11.incipit = "When the Cross was set…"
trMA.Ode6C11.text = "When the Cross was set up on the earth, the demons fell; and now, seeing it set before us in glory, let us kiss it and rise up from our fall into sm."

trMA.Ode6C12.ode = miscellanea_en_UK_ware.misc.Ode6
trMA.Ode6C12.mode = miscellanea_en_UK_ware.misc.Mode4
trMA.Ode6C12.melody = he.h.m8_en_UK_ware.heHE.IlasthitiMoiSotir.name
trMA.Ode6C12.incipit = "We exalt Thee, O Christ…"
trMA.Ode6C12.text = "We exalt Thee, O Christ, as our King and Lord; and with great rejoicing in the middle of the Fast we embrace the Cross, which Thou hast given us as a rampart that cannot be overthrown."

trMA.Ode6C13.ode = miscellanea_en_UK_ware.misc.Ode6
trMA.Ode6C13.mode = miscellanea_en_UK_ware.misc.Mode4
trMA.Ode6C13.melody = he.h.m8_en_UK_ware.heHE.IlasthitiMoiSotir.name
trMA.Ode6C13.incipit = "The Cross of the Lord…"
trMA.Ode6C13.text = "The Cross of the Lord lies here before us, bestowing great gifts on us all. Let us approach, and receive from it enlightenment of heart and soul."

trMA.Ode6C1T.ode = miscellanea_en_UK_ware.misc.Ode6
trMA.Ode6C1T.mode = miscellanea_en_UK_ware.misc.Mode4
trMA.Ode6C1T.melody = he.h.m8_en_UK_ware.heHE.IlasthitiMoiSotir.name
trMA.Ode6C1T.incipit = "Give us strength to fast…"
trMA.Ode6C1T.text = "Give us strength to fast from all evil, O pure Virgin, and enable us continually to refrain from sinful acts, for thou art the Protection of all men."

trMA.Kontakion1.mode = miscellanea_en_UK_ware.misc.Mode7
trMA.Kontakion1.melody = miscellanea_en_UK_ware.misc.Automelon
trMA.Kontakion1.incipit = tr.d043_en_UK_ware.trMA.Kontakion1.incipit
trMA.Kontakion1.text = tr.d043_en_UK_ware.trMA.Kontakion1.text

trMA.Ode7C1H.ode = miscellanea_en_UK_ware.misc.Ode7
trMA.Ode7C1H.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode7C1H.melody = he.h.m8_en_UK_ware.heHE.TheouSynkatavasin.melody
trMA.Ode7C1H.incipit = he.h.m8_en_UK_ware.heHE.TheouSynkatavasin.incipit
trMA.Ode7C1H.text = he.h.m8_en_UK_ware.heHE.TheouSynkatavasin.text

trMA.Ode7C11.ode = miscellanea_en_UK_ware.misc.Ode7
trMA.Ode7C11.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode7C11.melody = he.h.m8_en_UK_ware.heHE.TheouSynkatavasin.name
trMA.Ode7C11.incipit = "He who is outside time…"
trMA.Ode7C11.text = "He who is outside time has taken flesh within time, and in His love He heals our long-continuing passions through this season of fasting, during which the divine Cross is set before us for our sanctification."

trMA.Ode7C12.ode = miscellanea_en_UK_ware.misc.Ode7
trMA.Ode7C12.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode7C12.melody = he.h.m8_en_UK_ware.heHE.TheouSynkatavasin.name
trMA.Ode7C12.incipit = "We praise, exalt, worship and…"
trMA.Ode7C12.text = "We praise, exalt, worship and magnify Thy power, O Lord, for to us Thy servants Thou hast granted the divine Cross, our unending joy and the guardian of our souls and bodies."

trMA.Ode7C13.ode = miscellanea_en_UK_ware.misc.Ode7
trMA.Ode7C13.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode7C13.melody = he.h.m8_en_UK_ware.heHE.TheouSynkatavasin.name
trMA.Ode7C13.incipit = "In the day of judgement…"
trMA.Ode7C13.text = "In the day of judgement, O Lord, condemn me not for my sins. Cast me not away in shame from before Thy face, but in Thy surpassing love take pity on me and save me by Thy precious Cross."

trMA.Ode7C14.ode = miscellanea_en_UK_ware.misc.Ode7
trMA.Ode7C14.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode7C14.melody = he.h.m8_en_UK_ware.heHE.TheouSynkatavasin.name
trMA.Ode7C14.incipit = "With wood Moses sweetened the…"
trMA.Ode7C14.text = "With wood Moses sweetened the bitter waters, and so prefigured the grace that comes through thee, O Cross: for by thy power we are delivered from the bitterness of the passions. Give us the sweetness of thy grace, as we venerate thee now with contrite souls."

trMA.Ode7C1T.ode = miscellanea_en_UK_ware.misc.Ode7
trMA.Ode7C1T.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode7C1T.melody = he.h.m8_en_UK_ware.heHE.TheouSynkatavasin.name
trMA.Ode7C1T.incipit = "Through thine intercession, Lady, enlarge…"
trMA.Ode7C1T.text = "Through thine intercession, Lady, enlarge my narrow heart, for thou hast closed up all the deceitful snares of the adversary; guide me on the narrow way that leads to spaciousness of life, O Mother of God."

trMA.Ode8C1H.ode = miscellanea_en_UK_ware.misc.Ode8
trMA.Ode8C1H.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode8C1H.melody = he.h.m8_en_UK_ware.heHE.EptaplasiosKaminon.melody
trMA.Ode8C1H.incipit = he.h.m8_en_UK_ware.heHE.EptaplasiosKaminon.incipit
trMA.Ode8C1H.text = he.h.m8_en_UK_ware.heHE.EptaplasiosKaminon.text

trMA.Ode8C11.ode = miscellanea_en_UK_ware.misc.Ode8
trMA.Ode8C11.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode8C11.melody = he.h.m8_en_UK_ware.heHE.EptaplasiosKaminon.name
trMA.Ode8C11.incipit = " Once the Prophet Elisha…"
trMA.Ode8C11.text = " Once the Prophet Elisha drew the iron axe-head out of the river with a piece of wood, and so prefigured thee, O honoured Cross. For thou hast brought us from the depths of error to the firm ground of the truth, and this day we are counted worthy to behold and venerate thee with faith, gaining salvation."

trMA.Ode8C12.ode = miscellanea_en_UK_ware.misc.Ode8
trMA.Ode8C12.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode8C12.melody = he.h.m8_en_UK_ware.heHE.EptaplasiosKaminon.name
trMA.Ode8C12.incipit = "When Jacob blessed his children…"
trMA.Ode8C12.text = "When Jacob blessed his children, most plainly he prefigured thee, O honoured Cross. And all of us, who by grace are counted worthy to behold thee, draw near to thee with undoubting faith; touching thee, we receive rich blessing, and light and salvation and the remission of our sins."

trMA.Ode8C13.ode = miscellanea_en_UK_ware.misc.Ode8
trMA.Ode8C13.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode8C13.melody = he.h.m8_en_UK_ware.heHE.EptaplasiosKaminon.name
trMA.Ode8C13.incipit = "Clothed in the white raiment…"
trMA.Ode8C13.text = "Clothed in the white raiment of our virtuous actions, let us draw near rejoicing in the middle of the Fast; and let us embrace with faith the Cross of the Lord, that, guided by its strength, we may complete the course with eagerness and joyfully attain the divine Passion."

trMA.Ode8C1Tr.ode = miscellanea_en_UK_ware.misc.Ode8
trMA.Ode8C1Tr.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode8C1Tr.melody = he.h.m8_en_UK_ware.heHE.EptaplasiosKaminon.name
trMA.Ode8C1Tr.incipit = "Single in Nature, coeternal, one…"
trMA.Ode8C1Tr.text = "Single in Nature, coeternal, one in throne, simple in Essence yet distinct in Persons, unbegotten Father, Son and Holy Spirit, Uncreated Substance and Godhead, we all praise Thee, singing: O ye priests bless and ye people exalt Him above all for ever."

trMA.Ode8C1T.ode = miscellanea_en_UK_ware.misc.Ode8
trMA.Ode8C1T.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode8C1T.melody = he.h.m8_en_UK_ware.heHE.EptaplasiosKaminon.name
trMA.Ode8C1T.incipit = "'Now I see Thee as…"
trMA.Ode8C1T.text = "'Now I see Thee as an innocent lamb, hanging nailed to the Cross by lawless men, O my Son who wast before all things began,' cried the Holy Virgin, 'and I am weighed down by grief, afflicted by a mother's angnish.' With never-silent voices, as is right, we sing her praises for ever."

trMA.Ode8C21.ode = miscellanea_en_UK_ware.misc.Ode8
trMA.Ode8C21.mode = miscellanea_en_UK_ware.misc.Mode4
trMA.Ode8C21.melody = he.h.m4_en_UK_ware.heHE.PaidasEvageis.name
trMA.Ode8C21.incipit = "Beholding the Passion of Thee…"
trMA.Ode8C21.text = "Beholding the Passion of Thee who art impassible, the creation suffered with Thee, O longsuffering Saviour. The sun was quenched and the rocks were split; the whole earth was shaken and cried out in fear: O ye works of the Lord, praise ye the Lord and exalt Him above all for ever."

trMA.Ode8C22.ode = miscellanea_en_UK_ware.misc.Ode8
trMA.Ode8C22.mode = miscellanea_en_UK_ware.misc.Mode4
trMA.Ode8C22.melody = he.h.m4_en_UK_ware.heHE.PaidasEvageis.name
trMA.Ode8C22.incipit = " O my compassionate God…"
trMA.Ode8C22.text = " O my compassionate God, rebuke the wild beasts from the reeds, that trouble me within; for Thou hast accepted to be struck in shameful dishonour by a reed, desiring to restore to honour all that had fallen into disgrace through the transgression of old."

trMA.Ode8C23.ode = miscellanea_en_UK_ware.misc.Ode8
trMA.Ode8C23.mode = miscellanea_en_UK_ware.misc.Mode4
trMA.Ode8C23.melody = he.h.m4_en_UK_ware.heHE.PaidasEvageis.name
trMA.Ode8C23.incipit = "When I call to mind…"
trMA.Ode8C23.text = "When I call to mind, O most just Judge, Thy dread judgement seat, I am filled with fear and consternation, I tremble and lament, for I know that my actions deserve to be condemned. But, O Saviour, let not the multitude of my sins prove greater than the wealth of Thy compassion."

trMA.Ode8C2T.ode = miscellanea_en_UK_ware.misc.Ode8
trMA.Ode8C2T.mode = miscellanea_en_UK_ware.misc.Mode4
trMA.Ode8C2T.melody = he.h.m4_en_UK_ware.heHE.PaidasEvageis.name
trMA.Ode8C2T.incipit = "'I bore Thee without pangs…"
trMA.Ode8C2T.text = "'I bore Thee without pangs of childbirth, but now I suffer anguish at Thy Crucifixion, and my heart is set on fire. For Thou art pierced with nails, longsuffering Lord, and wounded in Thy side', lamented the all­ pure Virgin; and with one accord we sing her praises as Theotokos."

trMA.Ode8C31.ode = miscellanea_en_UK_ware.misc.Ode8
trMA.Ode8C31.mode = miscellanea_en_UK_ware.misc.Mode4
trMA.Ode8C31.melody = he.h.m4_en_UK_ware.heHE.PantaTaErgaTouTheou.name
trMA.Ode8C31.incipit = "Let the mountains spiritually break…"
trMA.Ode8C31.text = "Let the mountains spiritually break forth into righteousness and the little hills into rejoicing, as they behold the veneration of the Wood of the Cross that we exalt, O Christ, above all for ever."

trMA.Ode8C32.ode = miscellanea_en_UK_ware.misc.Ode8
trMA.Ode8C32.mode = miscellanea_en_UK_ware.misc.Mode4
trMA.Ode8C32.melody = he.h.m4_en_UK_ware.heHE.PantaTaErgaTouTheou.name
trMA.Ode8C32.incipit = "Fearful is the grace of…"
trMA.Ode8C32.text = "Fearful is the grace of Thy Cross: for it drives away the hosts of demons and brings healing to men. Therefore we sing Thy praises, O Christ, for ever."

trMA.Ode8C3Tr.ode = miscellanea_en_UK_ware.misc.Ode8
trMA.Ode8C3Tr.mode = miscellanea_en_UK_ware.misc.Mode4
trMA.Ode8C3Tr.melody = he.h.m4_en_UK_ware.heHE.PantaTaErgaTouTheou.name
trMA.Ode8C3Tr.incipit = "Let us glorify the Father…"
trMA.Ode8C3Tr.text = "Let us glorify the Father who is without beginning and the Son with the Holy Spirit, consubstantial Trinity, one sovereign power and God; and let us sing His praises for ever."

trMA.Ode8C3T.ode = miscellanea_en_UK_ware.misc.Ode8
trMA.Ode8C3T.mode = miscellanea_en_UK_ware.misc.Mode4
trMA.Ode8C3T.melody = he.h.m4_en_UK_ware.heHE.PantaTaErgaTouTheou.name
trMA.Ode8C3T.incipit = "Every mortal tongue sings thy…"
trMA.Ode8C3T.text = "Every mortal tongue sings thy praises, O Virgin, for from thee has shone out the Light that no man can approach, Christ our God, who illumines the world; and we exalt Him above all for ever."

trMA.Ode8C3A1.ode = miscellanea_en_UK_ware.misc.Ode8
trMA.Ode8C3A1.mode = miscellanea_en_UK_ware.misc.Mode4
trMA.Ode8C3A1.melody = he.h.m4_en_UK_ware.heHE.PantaTaErgaTouTheou.name
trMA.Ode8C3A1.incipit = "The ends of the earth…"
trMA.Ode8C3A1.text = "The ends of the earth rejoice at the veneration of Thy Cross, O Lord who lovest mankind. Angels in heaven make glad with them today,  O Christ, and join us in singing Thy praises for ever."

trMA.Ode8C3H.ode = miscellanea_en_UK_ware.misc.Ode8
trMA.Ode8C3H.mode = miscellanea_en_UK_ware.misc.Mode4
trMA.Ode8C3H.melody = he.h.m4_en_UK_ware.heHE.PantaTaErgaTouTheou.melody
trMA.Ode8C3H.incipit = he.h.m4_en_UK_ware.heHE.PantaTaErgaTouTheou.incipit
trMA.Ode8C3H.text = he.h.m4_en_UK_ware.heHE.PantaTaErgaTouTheou.text

trMA.Ode9C1H.ode = miscellanea_en_UK_ware.misc.Ode9
trMA.Ode9C1H.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode9C1H.melody = he.h.m8_en_UK_ware.heHE.ExestiEpiTouto.melody
trMA.Ode9C1H.incipit = he.h.m8_en_UK_ware.heHE.ExestiEpiTouto.incipit
trMA.Ode9C1H.text = he.h.m8_en_UK_ware.heHE.ExestiEpiTouto.text

trMA.Ode9C11.ode = miscellanea_en_UK_ware.misc.Ode9
trMA.Ode9C11.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode9C11.melody = he.h.m8_en_UK_ware.heHE.ExestiEpiTouto.name
trMA.Ode9C11.incipit = "Heal my brokenness, O King…"
trMA.Ode9C11.text = "Heal my brokenness, O King of all, crucified upon the Cross in Thy surpassing love. Thy hands and feet were pierced with nails, Thy side was wounded with the spear, and Thou wast given vinegar and gall to drink, who art the joy of all men, their sweetness, glory and eternal redemption."

trMA.Ode9C12.ode = miscellanea_en_UK_ware.misc.Ode9
trMA.Ode9C12.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode9C12.melody = he.h.m8_en_UK_ware.heHE.ExestiEpiTouto.name
trMA.Ode9C12.incipit = "O divine Cross, more beautiful…"
trMA.Ode9C12.text = "O divine Cross, more beautiful than sapphire and gold, thou shinest upon us with the brightness of the sun. We see thee surrounded in fear by the ranks of angels; and with the rays of Thy heavenly power thou givest light to the fullness of the inhabited earth."

trMA.Ode9C13.ode = miscellanea_en_UK_ware.misc.Ode9
trMA.Ode9C13.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode9C13.melody = he.h.m8_en_UK_ware.heHE.ExestiEpiTouto.name
trMA.Ode9C13.incipit = "The Cross is the haven…"
trMA.Ode9C13.text = "The Cross is the haven of the storm-tossed, the guide and support of those that go astray, the glory of Christ, the power of the apostles and the prophets, the strength of God's athletes, the refuge of all men. We see it set before us in this time of fasting and we venerate it."

trMA.Ode9C14.ode = miscellanea_en_UK_ware.misc.Ode9
trMA.Ode9C14.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode9C14.melody = he.h.m8_en_UK_ware.heHE.ExestiEpiTouto.name
trMA.Ode9C14.incipit = "When Thou shalt come on…"
trMA.Ode9C14.text = "When Thou shalt come on earth, O Lord, to judge the world which Thou hast fashioned, the angelic hosts will attend Thee, and the Cross will shine before Thee more brightly than the sun's rays. Through its power take pity on me then and save me, for I have sinned more than all mortal men."

trMA.Ode9C1T.ode = miscellanea_en_UK_ware.misc.Ode9
trMA.Ode9C1T.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode9C1T.melody = he.h.m8_en_UK_ware.heHE.ExestiEpiTouto.name
trMA.Ode9C1T.incipit = "'From my womb without corruption…"
trMA.Ode9C1T.text = "'From my womb without corruption have I borne Thee, who wast begotten from the Father before all ages: how, then, my Son, art Thou subject to corruption? For men rend Thy body, and in cruelty they pierce Thy side with a spear, Thy hands and feet with nails.' So the Holy Virgin cried; and, as is right, we magnify her."

trMA.Ode9C21.ode = miscellanea_en_UK_ware.misc.Ode9
trMA.Ode9C21.mode = miscellanea_en_UK_ware.misc.Mode4
trMA.Ode9C21.melody = he.h.m4_en_UK_ware.heHE.ApasGigenis.name
trMA.Ode9C21.incipit = "By the Wood of the…"
trMA.Ode9C21.text = "By the Wood of the Cross, O Christ rich in mercy, Thou hast made the flame of sin die down. With Thy pierced hands Thou hast delivered from the hand of the adversary him who in his greed stretched out his hand, to take the fruit of the only tree in Paradise from which Thou hadst forbidden him to eat."

trMA.Ode9C22.ode = miscellanea_en_UK_ware.misc.Ode9
trMA.Ode9C22.mode = miscellanea_en_UK_ware.misc.Mode4
trMA.Ode9C22.melody = he.h.m4_en_UK_ware.heHE.ApasGigenis.name
trMA.Ode9C22.incipit = "Count us worthy, Christ, with…"
trMA.Ode9C22.text = "Count us worthy, Christ, with purity of heart to venerate Thy precious Cross, and to see Thy Passion that brings freedom from the passions to all mortal men. Look not upon our sins, but make us true sons of Thy Resurrection."

trMA.Ode9C23.ode = miscellanea_en_UK_ware.misc.Ode9
trMA.Ode9C23.mode = miscellanea_en_UK_ware.misc.Mode4
trMA.Ode9C23.melody = he.h.m4_en_UK_ware.heHE.ApasGigenis.name
trMA.Ode9C23.incipit = "By Thy Resurrection Thou hast…"
trMA.Ode9C23.text = "By Thy Resurrection Thou hast raised us who were fallen into death; Thou hast destroyed death and reconciled us to Thy Father, shedding Thy blood for us, O Benefactor. Therefore we glorify Thee as almighty God, our Redeemer."

trMA.Ode9C2T.ode = miscellanea_en_UK_ware.misc.Ode9
trMA.Ode9C2T.mode = miscellanea_en_UK_ware.misc.Mode4
trMA.Ode9C2T.melody = he.h.m4_en_UK_ware.heHE.ApasGigenis.name
trMA.Ode9C2T.incipit = "O Virgin Theotokos, the strong…"
trMA.Ode9C2T.text = "O Virgin Theotokos, the strong refuge of the faithful, thou only hast conceived the very Person of God's Wisdom. I entreat thee, give me wisdom, that I may escape from the snares and cunning of him who is falsely wise in evil."

trMA.Ode9C31.ode = miscellanea_en_UK_ware.misc.Ode9
trMA.Ode9C31.mode = miscellanea_en_UK_ware.misc.Mode4
trMA.Ode9C31.melody = he.h.m4_en_UK_ware.heHE.TonAsporonTokonSou.name
trMA.Ode9C31.incipit = "We venerate and magnify Thy…"
trMA.Ode9C31.text = "We venerate and magnify Thy precious Cross, that is attended by the hosts of angels."

trMA.Ode9C32.ode = miscellanea_en_UK_ware.misc.Ode9
trMA.Ode9C32.mode = miscellanea_en_UK_ware.misc.Mode4
trMA.Ode9C32.melody = he.h.m4_en_UK_ware.heHE.TonAsporonTokonSou.name
trMA.Ode9C32.incipit = "We magnify the Saviour's Cross…"
trMA.Ode9C32.text = "We magnify the Saviour's Cross, the holy Wood, whereby we who before were dead partake of life."

trMA.Ode9C3Tr.ode = miscellanea_en_UK_ware.misc.Ode9
trMA.Ode9C3Tr.mode = miscellanea_en_UK_ware.misc.Mode4
trMA.Ode9C3Tr.melody = he.h.m4_en_UK_ware.heHE.TonAsporonTokonSou.name
trMA.Ode9C3Tr.incipit = "We magnify the Holy Trinity…"
trMA.Ode9C3Tr.text = "We magnify the Holy Trinity, Father without beginning, coeternal Son, and the Spirit who shares their throne."

trMA.Ode9C3T.ode = miscellanea_en_UK_ware.misc.Ode9
trMA.Ode9C3T.mode = miscellanea_en_UK_ware.misc.Mode4
trMA.Ode9C3T.melody = he.h.m4_en_UK_ware.heHE.TonAsporonTokonSou.name
trMA.Ode9C3T.incipit = "All of us sing thy…"
trMA.Ode9C3T.text = "All of us sing thy praises, Mother who hast not known man, Holy Virgin, for without seed thou hast conceived the Creator."

trMA.Ode9C3A1.ode = miscellanea_en_UK_ware.misc.Ode9
trMA.Ode9C3A1.mode = miscellanea_en_UK_ware.misc.Mode4
trMA.Ode9C3A1.melody = he.h.m4_en_UK_ware.heHE.TonAsporonTokonSou.name
trMA.Ode9C3A1.incipit = " O Cross of Christ…"
trMA.Ode9C3A1.text = " O Cross of Christ, we have been counted worthy to see and venerate thee. Guide us on our journey, that we may reach the holy Passion."

