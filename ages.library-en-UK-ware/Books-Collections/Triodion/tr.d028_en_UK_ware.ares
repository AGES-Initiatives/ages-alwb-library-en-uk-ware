trMA.Ode3C1H.ode = miscellanea_en_UK_ware.misc.Ode3
trMA.Ode3C1H.mode = miscellanea_en_UK_ware.misc.Mode4
trMA.Ode3C1H.melody = he.h.m4_en_UK_ware.TousSousYmnologous.melody
trMA.Ode3C1H.incipit = he.h.m4_en_UK_ware.TousSousYmnologous.incipit
