trMA.C1.verse = ""

trMA.Ode1C11.ode = misc_en_UK_ware.Ode1
trMA.Ode1C11.mode = misc_en_UK_ware.Mode1
trMA.Ode1C11.melody = he.h.m1_en_UK_ware.OdinEpinikion.incipit
trMA.Ode1C11.incipit = "Through abstinence let us put"
trMA.Ode1C11.text = "Through abstinence let us put to death the passions and through godlike actions let us bring to life the spirit, that with pure minds we may see the holy Passion of Christ."

trMA.Ode1C12.ode = misc_en_UK_ware.Ode1
trMA.Ode1C12.mode = misc_en_UK_ware.Mode1
trMA.Ode1C12.melody = he.h.m1_en_UK_ware.OdinEpinikion.incipit
trMA.Ode1C12.incipit = "O Christ of many mercies,"
trMA.Ode1C12.text = "O Christ of many mercies, I have followed the ways of the rich man who showed no compassion; but I pray Thee, make me to dwell with the poor man Lazarus, and deliver me from the flame and the fire that never shall be quenched."

trMA.Ode1C13.ode = misc_en_UK_ware.Ode1
trMA.Ode1C13.mode = misc_en_UK_ware.Mode1
trMA.Ode1C13.melody = he.h.m1_en_UK_ware.OdinEpinikion.incipit
trMA.Ode1C13.incipit = "O Lord, I have fasted"
trMA.Ode1C13.text = "O Lord, I have fasted from every act of virtue, and taken my fill of sin. Now I am hungry: O feed me with the holy food of salvation."

trMA.Ode1C1T.ode = misc_en_UK_ware.Ode1
trMA.Ode1C1T.mode = misc_en_UK_ware.Mode1
trMA.Ode1C1T.melody = he.h.m1_en_UK_ware.OdinEpinikion.incipit
trMA.Ode1C1T.incipit = "O undefiled Lady, I am"
trMA.Ode1C1T.text = "O undefiled Lady, I am condemned as an outcast because of my actions: save me and take pity on me, for thou hast borne God the allÂ­compassionate."

trMA.C2.title = ""
trMA.C2.verse = ""

trMA.Ode1C21.ode = misc_en_UK_ware.Ode1
trMA.Ode1C21.mode = misc_en_UK_ware.Mode1
trMA.Ode1C21.melody = he.h.m1_en_UK_ware.ToVoithisantiTheo.incipit
trMA.Ode1C21.incipit = "O ye faithful, let us"
trMA.Ode1C21.text = "O ye faithful, let us prepare to celebrate Palm Sunday, joyfully observing the forefeast from this present day onwards, that we may be counted worthy to see the lifegiving Passion."

trMA.Ode1C22.ode = misc_en_UK_ware.Ode1
trMA.Ode1C22.mode = misc_en_UK_ware.Mode1
trMA.Ode1C22.melody = he.h.m1_en_UK_ware.ToVoithisantiTheo.incipit
trMA.Ode1C22.incipit = "Christ draws near and comes"
trMA.Ode1C22.text = "Christ draws near and comes to Jerusalem, seated as King on the foal of an ass, that He may subdue the disobedient nations beneath the Father's yoke."

trMA.Ode1C2Tr.ode = misc_en_UK_ware.Ode1
trMA.Ode1C2Tr.mode = misc_en_UK_ware.Mode1
trMA.Ode1C2Tr.melody = he.h.m1_en_UK_ware.ToVoithisantiTheo.incipit
trMA.Ode1C2Tr.incipit = "One in Godhead, Unity in"
trMA.Ode1C2Tr.text = "One in Godhead, Unity in three Persons, God and Father unbegotten, only-begotten Son, one and only Holy Spirit, single sovereignty and power, save us all."

trMA.Ode1C2T.ode = misc_en_UK_ware.Ode1
trMA.Ode1C2T.mode = misc_en_UK_ware.Mode1
trMA.Ode1C2T.melody = he.h.m1_en_UK_ware.ToVoithisantiTheo.incipit
trMA.Ode1C2T.incipit = "Glorious things are spoken of"
trMA.Ode1C2T.text = "Glorious things are spoken of thee throughout all the world, for thou hast borne in the flesh the Maker of all, O Theotokos Mary who hast known no marriage and no bridal bed."

trMA.Ode1C2A1.ode = misc_en_UK_ware.Ode1
trMA.Ode1C2A1.mode = misc_en_UK_ware.Mode1
trMA.Ode1C2A1.melody = he.h.m1_en_UK_ware.ToVoithisantiTheo.incipit
trMA.Ode1C2A1.incipit = "Behold, Christ has come to"
trMA.Ode1C2A1.text = "Behold, Christ has come to the city of Bethphage. Rejoice, Bethany, home of Lazarus, for a great miracle shall He show to thee, raising Lazarus from the dead."

trMA.Ode1C2H.ode = misc_en_UK_ware.Ode1
trMA.Ode1C2H.mode = misc_en_UK_ware.Mode1
trMA.Ode1C2H.melody = misc_en_UK_ware.HeirmosNotMetered
trMA.Ode1C2H.incipit = he.h.m1_en_UK_ware.ToVoithisantiTheo.incipit
trMA.Ode1C2H.text = he.h.m1_en_UK_ware.ToVoithisantiTheo.text

trMA.Ode8C11.ode = misc_en_UK_ware.Ode8
trMA.Ode8C11.mode = misc_en_UK_ware.Mode1
trMA.Ode8C11.melody = he.h.m1_en_UK_ware.OnFrittousinAngeloi.incipit
trMA.Ode8C11.incipit = "In my wretchedness I have"
trMA.Ode8C11.text = "In my wretchedness I have grown rich in unprofitable thoughts, I have defiled my soul with lawless pleasures, and the darkness of despair surrounds me. O my God, shine upon me with the light of repentance."

trMA.Ode8C12.ode = misc_en_UK_ware.Ode8
trMA.Ode8C12.mode = misc_en_UK_ware.Mode1
trMA.Ode8C12.melody = he.h.m1_en_UK_ware.OnFrittousinAngeloi.incipit
trMA.Ode8C12.incipit = "As once Thou hast given"
trMA.Ode8C12.text = "As once Thou hast given light to the man blind from birth, shine upon my soul that does not see Thy Light, O merciful Lord, but lies buried in the darkness of sinful forgetfulness and in the cares of this life."

trMA.Ode8C13.ode = misc_en_UK_ware.Ode8
trMA.Ode8C13.mode = misc_en_UK_ware.Mode1
trMA.Ode8C13.melody = he.h.m1_en_UK_ware.OnFrittousinAngeloi.incipit
trMA.Ode8C13.incipit = "Through fasting Elijah opened the"
trMA.Ode8C13.text = "Through fasting Elijah opened the heavens and watered the thirsty earth with showers of rain. Let us fast, pouring out streams of tears from our soul, that we may be granted mercy."

trMA.Ode8C1T.ode = misc_en_UK_ware.Ode8
trMA.Ode8C1T.mode = misc_en_UK_ware.Mode1
trMA.Ode8C1T.melody = he.h.m1_en_UK_ware.OnFrittousinAngeloi.incipit
trMA.Ode8C1T.incipit = "As a living vine, pure"
trMA.Ode8C1T.text = "As a living vine, pure Virgin, hast thou borne for us the ripe Cluster of grapes, from which there flows the wine of forgiveness, drying up the drunkenness of sin."

trMA.Ode8C21.ode = misc_en_UK_ware.Ode8
trMA.Ode8C21.mode = misc_en_UK_ware.Mode1
trMA.Ode8C21.melody = he.h.m1_en_UK_ware.YmnonSoiProsferomen.incipit
trMA.Ode8C21.incipit = "The door of the forecourt"
trMA.Ode8C21.text = "The door of the forecourt is opened, that leads us to the raising of Lazarus: for Christ is come, to awaken the dead man as from sleep and to overthrow death by life."

trMA.Ode8C22.ode = misc_en_UK_ware.Ode8
trMA.Ode8C22.mode = misc_en_UK_ware.Mode1
trMA.Ode8C22.melody = he.h.m1_en_UK_ware.YmnonSoiProsferomen.incipit
trMA.Ode8C22.incipit = "Great is thy glory, O"
trMA.Ode8C22.text = "Great is thy glory, O Bethany! For thou art counted worthy to receive the Creator as thy guest, and thou dost cry aloud: O ye works of the Lord, bless ye the Lord."

trMA.Ode8C2Tr.ode = misc_en_UK_ware.Ode8
trMA.Ode8C2Tr.mode = misc_en_UK_ware.Mode1
trMA.Ode8C2Tr.melody = he.h.m1_en_UK_ware.YmnonSoiProsferomen.incipit
trMA.Ode8C2Tr.incipit = "Let us glorify Thee, O"
trMA.Ode8C2Tr.text = "Let us glorify Thee, O Trinity in Unity, supreme in Being, one in Essence yet distinct in Persons, Father, Son and Spirit of God."

trMA.Ode8C2T.ode = misc_en_UK_ware.Ode8
trMA.Ode8C2T.mode = misc_en_UK_ware.Mode1
trMA.Ode8C2T.melody = he.h.m1_en_UK_ware.YmnonSoiProsferomen.incipit
trMA.Ode8C2T.incipit = "With the angel we say"
trMA.Ode8C2T.text = "With the angel we say to thee 'Rejoice!', for thou hast brought into the world the Joy and Saviour of the world. O Virgin worthy of all praise, entreat Him for us all."

trMA.Ode8C2A1.ode = misc_en_UK_ware.Ode8
trMA.Ode8C2A1.mode = misc_en_UK_ware.Mode1
trMA.Ode8C2A1.melody = he.h.m1_en_UK_ware.YmnonSoiProsferomen.incipit
trMA.Ode8C2A1.incipit = "We offer unto Thee a"
trMA.Ode8C2A1.text = "We offer unto Thee a hymn; for Thou who ridest on high upon the cherubim ridest now upon a foal, bringing all men into subjection beneath Thy power, O Christ."

trMA.Ode8C2H.ode = misc_en_UK_ware.Ode8
trMA.Ode8C2H.mode = misc_en_UK_ware.Mode1
trMA.Ode8C2H.melody = misc_en_UK_ware.HeirmosNotMetered
trMA.Ode8C2H.incipit = he.h.m1_en_UK_ware.YmnonSoiProsferomen.incipit
trMA.Ode8C2H.text = he.h.m1_en_UK_ware.YmnonSoiProsferomen.text

trMA.Ode9C11.ode = misc_en_UK_ware.Ode9
trMA.Ode9C11.mode = misc_en_UK_ware.Mode1
trMA.Ode9C11.melody = he.h.m1_en_UK_ware.TinZoodochonPigin.incipit
trMA.Ode9C11.incipit = "Slain by my many sins,"
trMA.Ode9C11.text = "Slain by my many sins, I am imprisoned in the tomb of negligence, and upon me lies the stone of despair. Remove it in Thy mercy, O Christ, and raise me up as once Thou hast raised Lazarus."

trMA.Ode9C12.ode = misc_en_UK_ware.Ode9
trMA.Ode9C12.mode = misc_en_UK_ware.Mode1
trMA.Ode9C12.melody = he.h.m1_en_UK_ware.TinZoodochonPigin.incipit
trMA.Ode9C12.incipit = "Let us flee from the"
trMA.Ode9C12.text = "Let us flee from the example of the rich man, who was condemned to the fire that never shall be quenched, and let us acquire the patience of Lazarus in his troubles, that Jesus may comfort us and make us sharers in His Kingdom."

trMA.Ode9C13.ode = misc_en_UK_ware.Ode9
trMA.Ode9C13.mode = misc_en_UK_ware.Mode1
trMA.Ode9C13.melody = he.h.m1_en_UK_ware.TinZoodochonPigin.incipit
trMA.Ode9C13.incipit = "Let us observe with gladness"
trMA.Ode9C13.text = "Let us observe with gladness the Fast that kills our passions; let us steadfastly devote ourselves to prayer; let us weep and mourn and lament with all our heart, that when we depart hence, we may go to dwell with Abraham."

trMA.Ode9C1T.ode = misc_en_UK_ware.Ode9
trMA.Ode9C1T.mode = misc_en_UK_ware.Mode1
trMA.Ode9C1T.melody = he.h.m1_en_UK_ware.TinZoodochonPigin.incipit
trMA.Ode9C1T.incipit = "Divine treasure of virginity, the"
trMA.Ode9C1T.text = "Divine treasure of virginity, the only restoration of our forefathers, fountain flowing with the deep waters of compassion, excellency and beauty of Jacob: let us sing the praises of the Theotokos, for through her we have been saved."

trMA.Ode9C21.ode = misc_en_UK_ware.Ode9
trMA.Ode9C21.mode = misc_en_UK_ware.Mode1
trMA.Ode9C21.melody = he.h.m1_en_UK_ware.SeTinOratheisan.incipit
trMA.Ode9C21.incipit = "While He is staying beyond"
trMA.Ode9C21.text = "While He is staying beyond Jordan, Christ is told today that Lazarus is ill; and, knowing all things in advance, He says: 'This sickness is not unto death.'"

trMA.Ode9C22.ode = misc_en_UK_ware.Ode9
trMA.Ode9C22.mode = misc_en_UK_ware.Mode1
trMA.Ode9C22.melody = he.h.m1_en_UK_ware.SeTinOratheisan.incipit
trMA.Ode9C22.incipit = "Make ready, O Bethany, adorn"
trMA.Ode9C22.text = "Make ready, O Bethany, adorn thy gates for God, open wide thy courts; for behold, the Master shall enter with the apostles, bringing life to him whom thou hast nurtured."

trMA.Ode9C2Tr.ode = misc_en_UK_ware.Ode9
trMA.Ode9C2Tr.mode = misc_en_UK_ware.Mode1
trMA.Ode9C2Tr.melody = he.h.m1_en_UK_ware.SeTinOratheisan.incipit
trMA.Ode9C2Tr.incipit = "The host of six-winged seraphim"
trMA.Ode9C2Tr.text = "The host of six-winged seraphim glorifies Thee, O Unity in three Persons, undivided Essence; and in our hymns with all mankind we magnify Thee."

trMA.Ode9C2T.ode = misc_en_UK_ware.Ode9
trMA.Ode9C2T.mode = misc_en_UK_ware.Mode1
trMA.Ode9C2T.melody = he.h.m1_en_UK_ware.SeTinOratheisan.incipit
trMA.Ode9C2T.incipit = "In ways past all interpretation"
trMA.Ode9C2T.text = "In ways past all interpretation Thou wast begotten outside time from the Father, and born within time from Thy Mother for the salvation of all; and in our hymns, as Creator and Master, we magnify Thee."

trMA.Ode9C2A1.ode = misc_en_UK_ware.Ode9
trMA.Ode9C2A1.mode = misc_en_UK_ware.Mode1
trMA.Ode9C2A1.melody = he.h.m1_en_UK_ware.SeTinOratheisan.incipit
trMA.Ode9C2A1.incipit = "Bearing the branches of our"
trMA.Ode9C2A1.text = "Bearing the branches of our generosity and the palms of our purity, let us all make ready to meet Christ who comes to Jerusalem as our God."

trMA.Ode9C2H.ode = misc_en_UK_ware.Ode9
trMA.Ode9C2H.mode = misc_en_UK_ware.Mode1
trMA.Ode9C2H.melody = misc_en_UK_ware.HeirmosNotMetered
trMA.Ode9C2H.incipit = he.h.m1_en_UK_ware.SeTinOratheisan.incipit
trMA.Ode9C2H.text = he.h.m1_en_UK_ware.SeTinOratheisan.text

trMA.AposIdiomelon1.mode = misc_en_UK_ware.Mode5
trMA.AposIdiomelon1.melody = misc_en_UK_ware.Idiomelon
trMA.AposIdiomelon1.incipit = "Condemn me not, O Christ,"
trMA.AposIdiomelon1.text = "Condemn me not, O Christ, to the flames of Gehenna, as the rich man was condemned because he had neglected Lazarus; but grant even to me, who ask with tears, a drop of Thy lovingkindness, O God, and have mercy upon me."

trMA.AposMartyric.mode = misc_en_UK_ware.Mode5
trMA.AposMartyric.melody = ""
trMA.AposMartyric.incipit = "Blessed is the army of"
trMA.AposMartyric.text = "Blessed is the army of the heavenly King: for though the victorious martyrs were born of this earth, yet despising their bodies they made haste to attain to the angelic estate; and through their sufferings they were counted worthy of the honour of the bodiless angels. At their prayers, O Lord, send down upon us Thy great mercy."

trMA.AposTheotokion.mode = misc_en_UK_ware.Mode5
trMA.AposTheotokion.melody = ""
trMA.AposTheotokion.incipit = "O full of grace, be"
trMA.AposTheotokion.text = "O full of grace, be thou our mediatrix through thine intercessions. Pray for a multitude of mercies on our souls and for the forgiveness of our many sins."

trTE.Troparion.mode = misc_en_UK_ware.Mode6
trTE.Troparion.melody = ""
trTE.Troparion.incipit = "This is the fearful day,"
trTE.Troparion.text = "This is the fearful day, O God, the evening of which we did not expect to reach; but in Thy love for mankind Thou hast counted us worthy to behold it. O Thrice-Holy, glory to Thee."
trTE.Troparion2.incipit = ""
trTE.Troparion2.text = ""
