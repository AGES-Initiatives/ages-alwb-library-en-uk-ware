trMA.C1.title = //titles_en_UK_ware.ti.Canon1
trMA.C1.verse = //prayers_en_UK_ware.pr.verse_God.text

trMA.Ode1C11.ode = miscellanea_en_UK_ware.misc.Ode1
trMA.Ode1C11.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode1C11.greekmelody = he.h.m8_en_UK_ware.heHE.AsomenToKyrio.greekname
trMA.Ode1C11.melody = he.h.m8_en_UK_ware.heHE.AsomenToKyrio.name
trMA.Ode1C11.incipit = "With Thy divine command Thou…"
trMA.Ode1C11.text = "With Thy divine command Thou hast raised dead Lazarus, in Thy love for mankind; for Thou art the Fashioner and Guardian of Life."

trMA.Ode1C12.ode = miscellanea_en_UK_ware.misc.Ode1
trMA.Ode1C12.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode1C12.melody = he.h.m8_en_UK_ware.heHE.AsomenToKyrio.name
trMA.Ode1C12.incipit = "O immortal Lord, with Thy…"
trMA.Ode1C12.text = "O immortal Lord, with Thy word Thou hast raised Lazarus, four days dead, and by Thy power Thou hast destroyed the dark kingdom of hell."

trMA.Ode1C13.ode = miscellanea_en_UK_ware.misc.Ode1
trMA.Ode1C13.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode1C13.melody = he.h.m8_en_UK_ware.heHE.AsomenToKyrio.name
trMA.Ode1C13.incipit = "Thou hast given to all…"
trMA.Ode1C13.text = "Thou hast given to all, O Master, a proof of Thy transcendent Godhead, raising Lazarus from the dead on the fourth day."

trMA.Ode1C14.ode = miscellanea_en_UK_ware.misc.Ode1
trMA.Ode1C14.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode1C14.melody = he.h.m8_en_UK_ware.heHE.AsomenToKyrio.name
trMA.Ode1C14.incipit = "Today Bethany proclaims beforehand the…"
trMA.Ode1C14.text = "Today Bethany proclaims beforehand the Resurrection of Christ the Giver of Life, and it rejoices at the rising of Lazarus."

trMA.C2.title = ""
trMA.C2.verse = ""

trMA.Ode1C21.ode = miscellanea_en_UK_ware.misc.Ode1
trMA.Ode1C21.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode1C21.greekmelody = he.h.m8_en_UK_ware.heHE.YgranDiodefsas.greekname
trMA.Ode1C21.melody = he.h.m8_en_UK_ware.heHE.YgranDiodefsas.name
trMA.Ode1C21.incipit = "In the beginning Thou hast…"
trMA.Ode1C21.text = "In the beginning Thou hast brought all creation out of nothing, and Thou knowest the secrets of our hearts; and now as Master Thou dost foretell to Thy disciples the falling asleep of Lazarus."

trMA.Ode1C22.ode = miscellanea_en_UK_ware.misc.Ode1
trMA.Ode1C22.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode1C22.melody = he.h.m8_en_UK_ware.heHE.YgranDiodefsas.name
trMA.Ode1C22.incipit = "O Christ, Thou hast become…"
trMA.Ode1C22.text = "O Christ, Thou hast become man, taking human nature from the Virgin, and as man Thou hast asked where Lazarus was buried, although as God Thou wast not ignorant of this."

trMA.Ode1C23.ode = miscellanea_en_UK_ware.misc.Ode1
trMA.Ode1C23.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode1C23.melody = he.h.m8_en_UK_ware.heHE.YgranDiodefsas.name
trMA.Ode1C23.incipit = "O Word, giving us an…"
trMA.Ode1C23.text = "O Word, giving us an assurance of Thine own Resurrection, Thou hast raised Thy friend as if from sleep, though he had lain four days in the tomb and already stank."

trMA.Ode1C2T.ode = miscellanea_en_UK_ware.misc.Ode1
trMA.Ode1C2T.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode1C2T.melody = he.h.m8_en_UK_ware.heHE.YgranDiodefsas.name
trMA.Ode1C2T.incipit = "The hosts of angels and…"
trMA.Ode1C2T.text = "The hosts of angels and of mortal men praise thee, O Virgin Mother, without ceasing. For thou hast carried their Creator as a babe in thine arms."

trMA.Ode1K.ode = miscellanea_en_UK_ware.misc.Ode1
trMA.Ode1K.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode1K.melody = he.h.m8_en_UK_ware.heHE.YgranDiodefsas.melody
trMA.Ode1K.incipit = he.h.m8_en_UK_ware.heHE.YgranDiodefsas.incipit
trMA.Ode1K.text = he.h.m8_en_UK_ware.heHE.YgranDiodefsas.text

trMA.Ode3C1H.ode = miscellanea_en_UK_ware.misc.Ode3
trMA.Ode3C1H.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode3C1H.greekmelody = he.h.m8_en_UK_ware.heHE.SyEiToStereoma.greekname
trMA.Ode3C1H.melody = he.h.m8_en_UK_ware.heHE.SyEiToStereoma.melody
trMA.Ode3C1H.incipit = he.h.m8_en_UK_ware.heHE.SyEiToStereoma.incipit
trMA.Ode3C1H.text = he.h.m8_en_UK_ware.heHE.SyEiToStereoma.text

trMA.Ode3C11.ode = miscellanea_en_UK_ware.misc.Ode3
trMA.Ode3C11.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode3C11.greekmelody = he.h.m8_en_UK_ware.heHE.SyEiToStereoma.greekname
trMA.Ode3C11.melody = he.h.m8_en_UK_ware.heHE.SyEiToStereoma.name
trMA.Ode3C11.incipit = "Displaying Thy two energies, O…"
trMA.Ode3C11.text = "Displaying Thy two energies, O Saviour, Thou hast made manifest Thy two natures: for Thou art both God and man."

trMA.Ode3C12.ode = miscellanea_en_UK_ware.misc.Ode3
trMA.Ode3C12.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode3C12.melody = he.h.m8_en_UK_ware.heHE.SyEiToStereoma.name
trMA.Ode3C12.incipit = "Though Thou art the Abyss…"
trMA.Ode3C12.text = "Though Thou art the Abyss of knowledge, Thou dost ask where they have laid the body of Lazarus. For it was Thy purpose, O Giver of Life, to raise him from the dead."

trMA.Ode3C13.ode = miscellanea_en_UK_ware.misc.Ode3
trMA.Ode3C13.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode3C13.melody = he.h.m8_en_UK_ware.heHE.SyEiToStereoma.name
trMA.Ode3C13.incipit = "Going from one place to…"
trMA.Ode3C13.text = "Going from one place to another, Thou hast, as mortal man, appeared circumscribed; but, as God uncircumscribed, Thou fillest all things."

trMA.Ode3C14.ode = miscellanea_en_UK_ware.misc.Ode3
trMA.Ode3C14.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode3C14.melody = he.h.m8_en_UK_ware.heHE.SyEiToStereoma.name
trMA.Ode3C14.incipit = "At Thy divine word, Thou…"
trMA.Ode3C14.text = "At Thy divine word, Thou hast raised Lazarus, O Christ. I pray Thee, raise me also, dead through my many sins."

trMA.Ode3C21.ode = miscellanea_en_UK_ware.misc.Ode3
trMA.Ode3C21.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode3C21.greekmelody = he.h.m8_en_UK_ware.heHE.OuraniasApsidos.greekname
trMA.Ode3C21.melody = he.h.m8_en_UK_ware.heHE.OuraniasApsidos.name
trMA.Ode3C21.incipit = "O Lord who workest miracles…"
trMA.Ode3C21.text = "O Lord who workest miracles, standing in Bethany by the tomb of Lazarus, Thou hast shed tears for him in accordance with the law of nature, confirming the full reality of the flesh which Thou hast taken, O Jesus my God."

trMA.Ode3C22.ode = miscellanea_en_UK_ware.misc.Ode3
trMA.Ode3C22.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode3C22.melody = he.h.m8_en_UK_ware.heHE.OuraniasApsidos.name
trMA.Ode3C22.incipit = "Straightway Thou hast made the…"
trMA.Ode3C22.text = "Straightway Thou hast made the sorrow of Mary and Martha to cease, O Saviour, showing Thy sovereign authority. For, as Thou hast said, Thou art in truth the Resurrection and the Life and the Lord of all."

trMA.Ode3C23.ode = miscellanea_en_UK_ware.misc.Ode3
trMA.Ode3C23.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode3C23.melody = he.h.m8_en_UK_ware.heHE.OuraniasApsidos.name
trMA.Ode3C23.incipit = "O Lord, from the ranks…"
trMA.Ode3C23.text = "O Lord, from the ranks of the dead and the darkness of hell thou hast snatched Thy friend Lazarus, bound in his grave-clothes; and by Thine all-powerful word Thou hast broken open the gates of the kingdom of death."

trMA.Ode3C2T.ode = miscellanea_en_UK_ware.misc.Ode3
trMA.Ode3C2T.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode3C2T.melody = he.h.m8_en_UK_ware.heHE.OuraniasApsidos.name
trMA.Ode3C2T.incipit = "Taking up Thy dwelling in…"
trMA.Ode3C2T.text = "Taking up Thy dwelling in a Virgin, O Lord, Thou hast appeared to men incarnate, enabling them to see Thee with their eyes; Thou hast made her in very truth the Theotokos and the Succour of the faithful, O Thou who alone lovest mankind."

trMA.Ode3K.ode = miscellanea_en_UK_ware.misc.Ode3
trMA.Ode3K.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode3K.melody = he.h.m8_en_UK_ware.heHE.OuraniasApsidos.melody
trMA.Ode3K.incipit = he.h.m8_en_UK_ware.heHE.OuraniasApsidos.incipit
trMA.Ode3K.text = he.h.m8_en_UK_ware.heHE.OuraniasApsidos.text

trMA.Ode3Kathisma1.mode = miscellanea_en_UK_ware.misc.Mode4
trMA.Ode3Kathisma1.greekmelody = he.a.m4_en_UK_ware.heAU.KateplagiIosif.greekname
trMA.Ode3Kathisma1.melody = he.a.m4_en_UK_ware.heAU.KateplagiIosif.notmetered
trMA.Ode3Kathisma1.incipit = "The sisters of Lazarus stood…"
trMA.Ode3Kathisma1.text = "The sisters of Lazarus stood beside Christ and, lamenting with bitter tears, they said to Him: ‘O Lord, Lazarus is dead.’ And though as God He knew the place of burial, yet He asked them, ‘Where have ye laid him?’ Coming to the tomb, He called Lazarus that was four days dead; and he arose and worshipped the Lord who had raised him."

trMA.Ode3Kathisma2.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode3Kathisma2.greekmelody = he.a.m8_en_UK_ware.heAU.TinSofianKaiLogon.greekname
trMA.Ode3Kathisma2.melody = he.a.m8_en_UK_ware.heAU.TinSofianKaiLogon.notmetered
trMA.Ode3Kathisma2.incipit = "Foreknowing all things as Creator…"
trMA.Ode3Kathisma2.text = "Foreknowing all things as Creator, Thou hast warned the disciples at Bethany, saying: ‘Our friend Lazarus has fallen asleep today.’ And, though Thou wast not ignorant, Thou hast asked: ‘Where have ye laid him?’ Weeping as a man, Thou hast prayed to the Father; Thou hast called Thy friend Lazarus from hell,O Lord, and raised him when he had been four days dead. Therefore we cry to Thee: Accept, O Christ our God, the praise we dare to offer, and count us all worthy of Thy glory."

trMA.Ode4C1H.ode = miscellanea_en_UK_ware.misc.Ode4
trMA.Ode4C1H.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode4C1H.greekmelody = he.h.m8_en_UK_ware.heHE.EisakikoaKyrie.greekname
trMA.Ode4C1H.melody = he.h.m8_en_UK_ware.heHE.EisakikoaKyrie.melody
trMA.Ode4C1H.incipit = he.h.m8_en_UK_ware.heHE.EisakikoaKyrie.incipit
trMA.Ode4C1H.text = he.h.m8_en_UK_ware.heHE.EisakikoaKyrie.text

trMA.Ode4C11.ode = miscellanea_en_UK_ware.misc.Ode4
trMA.Ode4C11.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode4C11.greekmelody = he.h.m8_en_UK_ware.heHE.EisakikoaKyrie.greekname
trMA.Ode4C11.melody = he.h.m8_en_UK_ware.heHE.EisakikoaKyrie.name
trMA.Ode4C11.incipit = "Thou hast prayed to the…"
trMA.Ode4C11.text = "Thou hast prayed to the Father, not because Thou art in need of any help, but to fulfil the mystery of Thine Incarnation; and so, almighty Lord, Thou hast raised up a corpse that was four days dead."

trMA.Ode4C12.ode = miscellanea_en_UK_ware.misc.Ode4
trMA.Ode4C12.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode4C12.melody = he.h.m8_en_UK_ware.heHE.EisakikoaKyrie.name
trMA.Ode4C12.incipit = "Coeternal with the Father, the…"
trMA.Ode4C12.text = "Coeternal with the Father, the Word that was revealed from the beginning as God, now offers prayers as man, though it is He that receives the prayers of all."

trMA.Ode4C13.ode = miscellanea_en_UK_ware.misc.Ode4
trMA.Ode4C13.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode4C13.melody = he.h.m8_en_UK_ware.heHE.EisakikoaKyrie.name
trMA.Ode4C13.incipit = "O Saviour, Thy voice destroyed…"
trMA.Ode4C13.text = "O Saviour, Thy voice destroyed all the power of death, and the foundations of hell were shaken by Thy divine might."

trMA.Ode4C1T.ode = miscellanea_en_UK_ware.misc.Ode4
trMA.Ode4C1T.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode4C1T.melody = he.h.m8_en_UK_ware.heHE.EisakikoaKyrie.name
trMA.Ode4C1T.incipit = "Let us sing the praises…"
trMA.Ode4C1T.text = "Let us sing the praises of the Virgin, for she remained a virgin after bearing child, and she gave birth to Christ our God, and has delivered the world from error."

trMA.Ode4C21.ode = miscellanea_en_UK_ware.misc.Ode4
trMA.Ode4C21.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode4C21.greekmelody = he.h.m8_en_UK_ware.heHE.SyMouIschys.greekname
trMA.Ode4C21.melody = he.h.m8_en_UK_ware.heHE.SyMouIschys.name
trMA.Ode4C21.incipit = "As Shepherd, O Saviour and…"
trMA.Ode4C21.text = "As Shepherd, O Saviour and Creator, Thou hast snatched from the fierce and all-devouring wolf a man that had been four days dead; and through him, in Thy power as Lord, Thou hast revealed beforehand the universal glory of Thy Resurrection on the third day."

trMA.Ode4C22.ode = miscellanea_en_UK_ware.misc.Ode4
trMA.Ode4C22.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode4C22.melody = he.h.m8_en_UK_ware.heHE.SyMouIschys.name
trMA.Ode4C22.incipit = "Beholding Thee, O Christ the…"
trMA.Ode4C22.text = "Beholding Thee, O Christ the Life, the companions of Martha cried aloud: ‘If Thou hadst been here, O Lord, the Light and Life of all, Lazarus would not have died.’ But since Thou art the Life of the dead, in Thy love for mankind Thou hast turned their sorrow into joy."

trMA.Ode4C23.ode = miscellanea_en_UK_ware.misc.Ode4
trMA.Ode4C23.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode4C23.melody = he.h.m8_en_UK_ware.heHE.SyMouIschys.name
trMA.Ode4C23.incipit = "The depths are afraid at…"
trMA.Ode4C23.text = "The depths are afraid at Thy presence, O Lord, the source of life; all the waters are Thy servants. The gatekeepers tremble before Thee, O Christ, and the bars of hell are broken by Thy power, as Lazarus rises from the dead at Thy command, almighty Saviour who lovest mankind."

trMA.Ode4C2T.ode = miscellanea_en_UK_ware.misc.Ode4
trMA.Ode4C2T.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode4C2T.melody = he.h.m8_en_UK_ware.heHE.SyMouIschys.name
trMA.Ode4C2T.incipit = "Unwedded Virgin, thou art the…"
trMA.Ode4C2T.text = "Unwedded Virgin, thou art the glory of the faithful; thou art the advocate and refuge of Christians, their rampart and haven. For, Undefiled, thou dost offer intercession to thy Son, saving from danger those who in faith and love acknowledge thee as the pure Theotokos."

trMA.Ode4K.ode = miscellanea_en_UK_ware.misc.Ode4
trMA.Ode4K.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode4K.melody = he.h.m8_en_UK_ware.heHE.SyMouIschys.melody
trMA.Ode4K.incipit = he.h.m8_en_UK_ware.heHE.SyMouIschys.incipit
trMA.Ode4K.text = he.h.m8_en_UK_ware.heHE.SyMouIschys.text

trMA.Ode5C1H.ode = miscellanea_en_UK_ware.misc.Ode5
trMA.Ode5C1H.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode5C1H.greekmelody = he.h.m8_en_UK_ware.heHE.InaTiMeAposo.greekname
trMA.Ode5C1H.melody = he.h.m8_en_UK_ware.heHE.InaTiMeAposo.melody
trMA.Ode5C1H.incipit = he.h.m8_en_UK_ware.heHE.InaTiMeAposo.incipit
trMA.Ode5C1H.text = he.h.m8_en_UK_ware.heHE.InaTiMeAposo.text

trMA.Ode5C11.ode = miscellanea_en_UK_ware.misc.Ode5
trMA.Ode5C11.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode5C11.greekmelody = he.h.m8_en_UK_ware.heHE.InaTiMeAposo.greekname
trMA.Ode5C11.melody = he.h.m8_en_UK_ware.heHE.InaTiMeAposo.name
trMA.Ode5C11.incipit = "In Thy love for mankind…"
trMA.Ode5C11.text = "In Thy love for mankind, coming to the tomb of Lazarus Thou hast called him and granted him life, for Thou art the immortal life of all mortal men; and so Thou hast as God clearly foretold the future Resurrection."

trMA.Ode5C12.ode = miscellanea_en_UK_ware.misc.Ode5
trMA.Ode5C12.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode5C12.melody = he.h.m8_en_UK_ware.heHE.InaTiMeAposo.name
trMA.Ode5C12.incipit = "His feet bound in the…"
trMA.Ode5C12.text = "His feet bound in the grave-clothes, Lazarus walked out from the tomb. O wonder of wonders! Christ who gave him strength is greater than the power of death which held him back. All things obey Christ’s word, serving Him as God and Master."

trMA.Ode5C13.ode = miscellanea_en_UK_ware.misc.Ode5
trMA.Ode5C13.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode5C13.melody = he.h.m8_en_UK_ware.heHE.InaTiMeAposo.name
trMA.Ode5C13.incipit = "Thou hast raised Lazarus on…"
trMA.Ode5C13.text = "Thou hast raised Lazarus on the fourth day, though his corpse already stank. Raise me up, O Christ, for I am dead in sin and lie in the pit and the dark shadow of death; deliver and save me in Thy compassion."

trMA.Ode5C21.ode = miscellanea_en_UK_ware.misc.Ode5
trMA.Ode5C21.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode5C21.greekmelody = he.h.m8_en_UK_ware.heHE.InaTiMeAposo.greekname
trMA.Ode5C21.melody = he.h.m8_en_UK_ware.heHE.InaTiMeAposo.name
trMA.Ode5C21.incipit = "Thou hast prayed and given…"
trMA.Ode5C21.text = "Thou hast prayed and given glory to the Father, for Thy power is not opposed to His. To confirm the faith of the multitude that stood round Thee, Thou hast thanked Thy Father, O longsuffering Lord, and then raised Lazarus by Thy command."

trMA.Ode5C22.ode = miscellanea_en_UK_ware.misc.Ode5
trMA.Ode5C22.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode5C22.melody = he.h.m8_en_UK_ware.heHE.InaTiMeAposo.name
trMA.Ode5C22.incipit = "O voice of God! O…"
trMA.Ode5C22.text = "O voice of God! O divine power and might! With that voice, O Saviour, Thou hast shattered the gates of hell and all-devouring death. Deliver me from my passions, as once Thou hast delivered Thy friend Lazarus that was four days dead."

trMA.Ode5C23.ode = miscellanea_en_UK_ware.misc.Ode5
trMA.Ode5C23.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode5C23.melody = he.h.m8_en_UK_ware.heHE.InaTiMeAposo.name
trMA.Ode5C23.incipit = "At the prayers of Lazarus…"
trMA.Ode5C23.text = "At the prayers of Lazarus, of Martha and Mary, O loving Lord, make us worthy to behold Thy Cross and Passion, and the joyful Queen of Days, the Feast of Thy Resurrection."

trMA.Ode5C2T.ode = miscellanea_en_UK_ware.misc.Ode5
trMA.Ode5C2T.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode5C2T.melody = he.h.m8_en_UK_ware.heHE.InaTiMeAposo.name
trMA.Ode5C2T.incipit = "Since all-pure Virgin, thou hast…"
trMA.Ode5C2T.text = "Since all-pure Virgin, thou hast a mother’s boldness before thy Son, do not forget us and our needs, we pray, for we are thy kinsfolk: thee alone we Christians bring as intercessor, to win the gracious mercy of the Master."

trMA.Ode5K.ode = miscellanea_en_UK_ware.misc.Ode5
