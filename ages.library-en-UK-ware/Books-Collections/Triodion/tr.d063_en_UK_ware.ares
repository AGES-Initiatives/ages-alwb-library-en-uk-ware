trMA.Ode8C1H.incipit = he.h.m8_en_UK_ware.heHE.MousikonOrganon.incipit
trMA.Ode8C1H.text = he.h.m8_en_UK_ware.heHE.MousikonOrganon.text

trMA.Ode8C11.ode = miscellanea_en_UK_ware.misc.Ode8
trMA.Ode8C11.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode8C11.greekmelody = he.h.m8_en_UK_ware.heHE.MousikonOrganon.greekname
trMA.Ode8C11.melody = he.h.m8_en_UK_ware.heHE.MousikonOrganon.name
trMA.Ode8C11.incipit = "As Shepherd, Thou hast gone…"
trMA.Ode8C11.text = "As Shepherd, Thou hast gone to seek Thy sheep; rescuing the dead man from the fierce and destructive wolf, in Thy power Thou hast brought him from corruption to new life, and he cried out to Thee: ‘Praise the Lord and exalt Him above all for ever.’"

trMA.Ode8C12.ode = miscellanea_en_UK_ware.misc.Ode8
trMA.Ode8C12.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode8C12.melody = he.h.m8_en_UK_ware.heHE.MousikonOrganon.name
trMA.Ode8C12.incipit = "As mortal man Thou hast…"
trMA.Ode8C12.text = "As mortal man Thou hast asked where Lazarus was buried; as Maker, Thou hast raised him from the dead by Thy royal command. Hell was afraid of him when he cried out to Thee: ‘Praise the Lord and exalt Him above all for ever.’"

trMA.Ode8C21.ode = miscellanea_en_UK_ware.misc.Ode8
trMA.Ode8C21.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode8C21.greekmelody = he.h.m8_en_UK_ware.heHE.TonVasileaTonOuranon.greekname
trMA.Ode8C21.melody = he.h.m8_en_UK_ware.heHE.TonVasileaTonOuranon.name
trMA.Ode8C21.incipit = "As a mortal, Thou dost…"
trMA.Ode8C21.text = "As a mortal, Thou dost search for Lazarus; as God, Thou dost raise him by Thy word, though he was four days dead. Therefore we sing Thy praises for ever."

trMA.Ode8C22.ode = miscellanea_en_UK_ware.misc.Ode8
trMA.Ode8C22.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode8C22.melody = he.h.m8_en_UK_ware.heHE.TonVasileaTonOuranon.name
trMA.Ode8C22.incipit = "Fulfilling a debt of gratitude…"
trMA.Ode8C22.text = "Fulfilling a debt of gratitude for her brother, Mary brought Thee, O Lord, sweet-smelling spices; and she sings Thy praises for ever."

trMA.Ode8C23.ode = miscellanea_en_UK_ware.misc.Ode8
trMA.Ode8C23.mode = miscellanea_en_UK_ware.misc.Mode8
trMA.Ode8C23.melody = he.h.m8_en_UK_ware.heHE.TonVasileaTonOuranon.name
trMA.Ode8C23.incipit = "As man Thou dost pray…"
trMA.Ode8C23.text = "As man Thou dost pray to the Father, as God Thou dost raise Lazarus. Therefore, O Christ, we sing Thy praises for ever."

