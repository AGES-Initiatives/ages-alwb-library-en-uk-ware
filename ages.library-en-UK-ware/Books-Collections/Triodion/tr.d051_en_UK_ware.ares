trMA.AposTheotokion.melody = ""
trMA.AposTheotokion.incipit = tr.d044_en_UK_ware.trVE.AposTheotokion.incipit
trMA.AposTheotokion.text = tr.d044_en_UK_ware.trVE.AposTheotokion.text

trTE.Troparion.mode = misc_en_UK_ware.Mode3
trTE.Troparion.melody = ""
trTE.Troparion.incipit = "Our iniquities have risen up…"
trTE.Troparion.text = "Our iniquities have risen up against us, but do Thou, O Lord, rise up and help us: for Thou art our Father, and we know none other save Thee."
trTE.Troparion2.incipit = "…"
trTE.Troparion2.text = ""
