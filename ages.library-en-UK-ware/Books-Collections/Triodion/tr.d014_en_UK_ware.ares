trMA.Ode1C11.ode = misc_en_UK_ware.Ode1
trMA.Ode1C11.mode = misc_en_UK_ware.Mode8
trMA.Ode1C11.melody = he.h.m8_en_UK_ware.AsmaAnapempsomen.name
trMA.Ode1C11.incipit = "As we celebrate today the…"
trMA.Ode1C11.text = "As we celebrate today the memory of the dead from the beginning, let us all entreat Christ to deliver from the everlasting fire those who have fallen asleep in faith and in the hope of eternal life."

trMA.Ode1C12.ode = misc_en_UK_ware.Ode1
trMA.Ode1C12.mode = misc_en_UK_ware.Mode8
trMA.Ode1C12.melody = he.h.m8_en_UK_ware.AsmaAnapempsomen.name
trMA.Ode1C12.incipit = "In the depth of Thy…"
trMA.Ode1C12.text = "In the depth of Thy judgements, O Christ, with fullness of wisdom Thou hast preordained the end of each man's life, its appointed moment and its manner. Therefore, All-Merciful, at the judgement save those in every land whom the grave has hidden."

trMA.Ode1C13.ode = misc_en_UK_ware.Ode1
trMA.Ode1C13.mode = misc_en_UK_ware.Mode8
trMA.Ode1C13.melody = he.h.m8_en_UK_ware.AsmaAnapempsomen.name
trMA.Ode1C13.incipit = "It is Thou who hast…"
trMA.Ode1C13.text = "It is Thou who hast set bounds to our time upon this earth; therefore, when we waken from the night of life, make us children of the day that knows no evening: Orthodox priests and kings and all Thy people."

trMA.Ode1C14.ode = misc_en_UK_ware.Ode1
trMA.Ode1C14.mode = misc_en_UK_ware.Mode8
trMA.Ode1C14.melody = he.h.m8_en_UK_ware.AsmaAnapempsomen.name
trMA.Ode1C14.incipit = "To those hidden by the…"
trMA.Ode1C14.text = "To those hidden by the deep or cut down in battle, swallowed by earthquake, murdered, or consumed by fire, grant in Thy mercy a place with the faithful and the righteous."

trMA.Ode1C15.ode = misc_en_UK_ware.Ode1
trMA.Ode1C15.mode = misc_en_UK_ware.Mode8
trMA.Ode1C15.melody = he.h.m8_en_UK_ware.AsmaAnapempsomen.name
trMA.Ode1C15.incipit = "Overlooking all the transgressions of…"
trMA.Ode1C15.text = "Overlooking all the transgressions of the flesh, our Saviour, in every age, by every nation of mankind, grant that, as they make their defence to Thee the Creator, they may stand before Thy judgement­seat uncondemned."

trMA.Ode1C1Tr.ode = misc_en_UK_ware.Ode1
trMA.Ode1C1Tr.mode = misc_en_UK_ware.Mode8
trMA.Ode1C1Tr.melody = he.h.m8_en_UK_ware.AsmaAnapempsomen.name
trMA.Ode1C1Tr.incipit = "I sing the praises of…"
trMA.Ode1C1Tr.text = "I sing the praises of three self-dependent Persons in one Nature, the Father unbegotten, the Son begotten, and the Holy Spirit: sovereignty and power without beginning, a single Godhead."

trMA.Ode1C1T.ode = misc_en_UK_ware.Ode1
trMA.Ode1C1T.mode = misc_en_UK_ware.Mode8
trMA.Ode1C1T.melody = he.h.m8_en_UK_ware.AsmaAnapempsomen.name
trMA.Ode1C1T.incipit = "Truly thou hast appeared as…"
trMA.Ode1C1T.text = "Truly thou hast appeared as heaven on earth, a heaven far greater than the highest firmament, O Virgin unwedded: For from thee has shone forth upon the world the Sun and King of righteousness."

trMA.Ode1K.ode = misc_en_UK_ware.Ode1
trMA.Ode1K.mode = misc_en_UK_ware.Mode8
trMA.Ode1K.melody = misc_en_UK_ware.HeirmosNotMetered
trMA.Ode1K.incipit = he.h.m8_en_UK_ware.AsmaAnapempsomen.incipit
trMA.Ode1K.text = he.h.m8_en_UK_ware.AsmaAnapempsomen.text

trMA.Ode2C1H.ode = misc_en_UK_ware.Ode2
trMA.Ode2C1H.mode = misc_en_UK_ware.Mode8
trMA.Ode2C1H.melody = misc_en_UK_ware.HeirmosNotMetered
trMA.Ode2C1H.incipit = he.h.m8_en_UK_ware.IdeteIdeteOProTonAionon.incipit
trMA.Ode2C1H.text = he.h.m8_en_UK_ware.IdeteIdeteOProTonAionon.text

trMA.Ode2C11.ode = misc_en_UK_ware.Ode2
trMA.Ode2C11.mode = misc_en_UK_ware.Mode8
trMA.Ode2C11.melody = he.h.m8_en_UK_ware.IdeteIdeteOProTonAionon.name
trMA.Ode2C11.incipit = "See now, see that I…"
trMA.Ode2C11.text = "See now, see that I am your God, who in righteous judgement have fixed the bounds of life, bringing from corruption into incorruption all that have fallen asleep in the hope of eternal resurrection."

trMA.Ode2C12.ode = misc_en_UK_ware.Ode2
trMA.Ode2C12.mode = misc_en_UK_ware.Mode8
trMA.Ode2C12.melody = he.h.m8_en_UK_ware.IdeteIdeteOProTonAionon.name
trMA.Ode2C12.incipit = "O Lord, Thou receivest from…"
trMA.Ode2C12.text = "O Lord, Thou receivest from the four corners of the earth those that have died in faith, at sea and on the land, in rivers, springs, lakes or wells, devoured by wild beasts, birds or creeping things. Give rest to them all."

trMA.Ode2C13.ode = misc_en_UK_ware.Ode2
trMA.Ode2C13.mode = misc_en_UK_ware.Mode8
trMA.Ode2C13.melody = he.h.m8_en_UK_ware.IdeteIdeteOProTonAionon.name
trMA.Ode2C13.incipit = "O Lord, in the palm…"
trMA.Ode2C13.text = "O Lord, in the palm of Thy hand Thou hast discerned all things before they come to pass. All those whose bodies are dissolved into the four elements, do Thou restore and raise up at Thy coming, for­ giving them all their offences committed in knowledge or in ignorance."

trMA.Ode2C14.ode = misc_en_UK_ware.Ode2
trMA.Ode2C14.mode = misc_en_UK_ware.Mode8
trMA.Ode2C14.melody = he.h.m8_en_UK_ware.IdeteIdeteOProTonAionon.name
trMA.Ode2C14.incipit = "O Lord, how fearful shall…"
trMA.Ode2C14.text = "O Lord, how fearful shall be Thy second Advent! For as lightning shalt Thou come on earth and raise up all Thy creatures to be judged. Grant to those who lived with faith in Thee, that they may meet Thee on that day and be counted worthy to dwell with Thee."

trMA.Ode2C1Tr.ode = misc_en_UK_ware.Ode2
trMA.Ode2C1Tr.mode = misc_en_UK_ware.Mode8
trMA.Ode2C1Tr.melody = he.h.m8_en_UK_ware.IdeteIdeteOProTonAionon.name
trMA.Ode2C1Tr.incipit = "Unity in three Persons, supreme…"
trMA.Ode2C1Tr.text = "Unity in three Persons, supreme in Godhead and surpassing all perfection, Father unbegotten, Son only-begotten, Spirit proceeding from the Father and made manifest through the Son: single in essence and in nature, one Lordship and one Kingdom, save us all."

trMA.Ode2C1T.ode = misc_en_UK_ware.Ode2
trMA.Ode2C1T.mode = misc_en_UK_ware.Mode8
trMA.Ode2C1T.melody = he.h.m8_en_UK_ware.IdeteIdeteOProTonAionon.name
trMA.Ode2C1T.incipit = "The wonder of thy conceiving…"
trMA.Ode2C1T.text = "The wonder of thy conceiving is past speech, O Mother and Virgin: for how hast thou given birth and yet remained undefiled? How dost thou bear child, without knowing a man? All this is understood by the Word of God, who was born from thee in a new way, surpassing nature."

trMA.Ode2K.ode = misc_en_UK_ware.Ode2
trMA.Ode2K.mode = misc_en_UK_ware.Mode8
trMA.Ode2K.melody = misc_en_UK_ware.HeirmosNotMetered
trMA.Ode2K.incipit = he.h.m8_en_UK_ware.IdeteIdeteOProTonAionon.incipit
trMA.Ode2K.text = he.h.m8_en_UK_ware.IdeteIdeteOProTonAionon.text

trMA.Ode3C1H.ode = misc_en_UK_ware.Ode3
trMA.Ode3C1H.mode = misc_en_UK_ware.Mode8
trMA.Ode3C1H.melody = misc_en_UK_ware.HeirmosNotMetered
trMA.Ode3C1H.incipit = he.h.m8_en_UK_ware.OStereosasEnTiCheiriSou.incipit
trMA.Ode3C1H.text = he.h.m8_en_UK_ware.OStereosasEnTiCheiriSou.text

trMA.Ode3C11.ode = misc_en_UK_ware.Ode3
trMA.Ode3C11.mode = misc_en_UK_ware.Mode8
trMA.Ode3C11.melody = he.h.m8_en_UK_ware.OStereosasEnTiCheiriSou.name
trMA.Ode3C11.incipit = "To those who have passed…"
trMA.Ode3C11.text = "To those who have passed through the course of life in the glory of the faith, grant a crown of righteousness, O God, and count them worthy to enjoy eternal blessings."

trMA.Ode3C12.ode = misc_en_UK_ware.Ode3
trMA.Ode3C12.mode = misc_en_UK_ware.Mode8
trMA.Ode3C12.melody = he.h.m8_en_UK_ware.OStereosasEnTiCheiriSou.name
trMA.Ode3C12.incipit = "To those who have been…"
trMA.Ode3C12.text = "To those who have been suddenly snatched away, burnt up by lightning, frozen by the cold, or struck down by any other calamity, give rest, O God, when Thou shalt make trial of all things in the fire."

trMA.Ode3C13.ode = misc_en_UK_ware.Ode3
trMA.Ode3C13.mode = misc_en_UK_ware.Mode8
trMA.Ode3C13.melody = he.h.m8_en_UK_ware.OStereosasEnTiCheiriSou.name
trMA.Ode3C13.incipit = "To those who have sailed…"
trMA.Ode3C13.text = "To those who have sailed across the ever-troubled sea of this earthly life steered by the Orthodox faith, grant a safe anchorage, O Christ, in the harbour of immortal life with Thee."

trMA.Ode3C14.ode = misc_en_UK_ware.Ode3
trMA.Ode3C14.mode = misc_en_UK_ware.Mode8
trMA.Ode3C14.melody = he.h.m8_en_UK_ware.OStereosasEnTiCheiriSou.name
trMA.Ode3C14.incipit = "Those whom the creatures of…"
trMA.Ode3C14.text = "Those whom the creatures of the sea or the birds of the air have devoured, O Christ our God, raise up in glory on the Last Day, as Thou judgest right."

trMA.Ode3C1Tr.ode = misc_en_UK_ware.Ode3
trMA.Ode3C1Tr.mode = misc_en_UK_ware.Mode8
trMA.Ode3C1Tr.melody = he.h.m8_en_UK_ware.OStereosasEnTiCheiriSou.name
trMA.Ode3C1Tr.incipit = "In my thought I distinguish…"
trMA.Ode3C1Tr.text = "In my thought I distinguish three Persons within the simplicity of the divine Oneness, and at the same time I join them together; for, shining forth in threefold radiance with the swiftness of lightning, the Godhead is made manifest as Unity."

trMA.Ode3C1T.ode = misc_en_UK_ware.Ode3
trMA.Ode3C1T.mode = misc_en_UK_ware.Mode8
trMA.Ode3C1T.melody = he.h.m8_en_UK_ware.OStereosasEnTiCheiriSou.name
trMA.Ode3C1T.incipit = "Mind cannot comprehend this wonder…"
trMA.Ode3C1T.text = "Mind cannot comprehend this wonder brought to pass in thee. For thou dost conceive without a man, pure Lady, and thou givest birth while keeping thy virginity. Therefore the angelic hosts and mortal men sing thy praise unto all ages."

trMA.Ode3K.ode = misc_en_UK_ware.Ode3
trMA.Ode3K.mode = misc_en_UK_ware.Mode8
trMA.Ode3K.melody = misc_en_UK_ware.HeirmosNotMetered
trMA.Ode3K.incipit = he.h.m8_en_UK_ware.OStereosasEnTiCheiriSou.incipit
trMA.Ode3K.text = he.h.m8_en_UK_ware.OStereosasEnTiCheiriSou.text

trMA.Ode3Kathisma1.mode = misc_en_UK_ware.Mode5
trMA.Ode3Kathisma1.melody = he.a.m5_en_UK_ware.TonSynanarchonLogon.name
trMA.Ode3Kathisma1.incipit = "O Saviour, who for our…"
trMA.Ode3Kathisma1.text = "O Saviour, who for our sakes hast endured the Cross and death, who hast put hell to death and raised the dead, give rest, O God, in Thy love for mankind, to those who have departed from us; and at Thy dread and fearful Coming, O Giver of Life, in the multitude of Thy mercies count them worthy of Thy Kingdom."

trMA.Ode3Kathisma2.mode = misc_en_UK_ware.Mode5
trMA.Ode3Kathisma2.melody = he.a.m5_en_UK_ware.TonSynanarchonLogon.name
trMA.Ode3Kathisma2.incipit = "Grant thy swift protection, thy…"
trMA.Ode3Kathisma2.text = "Grant thy swift protection, thy help and mercy, to thy servants; calm the waves of my vain thoughts, O pure Theotokos, and raise up my fallen soul. For I know, I know, O Virgin, that thou hast power for all thou dost desire."

trMA.Ode4C1H.ode = misc_en_UK_ware.Ode4
trMA.Ode4C1H.mode = misc_en_UK_ware.Mode8
trMA.Ode4C1H.melody = misc_en_UK_ware.HeirmosNotMetered
trMA.Ode4C1H.incipit = he.h.m8_en_UK_ware.ExOrousKataskiou.incipit
trMA.Ode4C1H.text = he.h.m8_en_UK_ware.ExOrousKataskiou.text

trMA.Ode4C11.ode = misc_en_UK_ware.Ode4
trMA.Ode4C11.mode = misc_en_UK_ware.Mode8
trMA.Ode4C11.melody = he.h.m8_en_UK_ware.ExOrousKataskiou.name
trMA.Ode4C11.incipit = "Fathers and forefathers, grandfathers and…"
trMA.Ode4C11.text = "Fathers and forefathers, grandfathers and great-grandfathers, those who from the beginning up to these last times have died in holiness of life and in true faith: remember them all, our Saviour."

trMA.Ode4C12.ode = misc_en_UK_ware.Ode4
trMA.Ode4C12.mode = misc_en_UK_ware.Mode8
trMA.Ode4C12.melody = he.h.m8_en_UK_ware.ExOrousKataskiou.name
trMA.Ode4C12.incipit = "Those who died in faith…"
trMA.Ode4C12.text = "Those who died in faith on the mountainside or the road, in the tombs or the desert, monks and married people, young and old: grant to them all, O Christ, a dwelling with Thy saints."

trMA.Ode4C13.ode = misc_en_UK_ware.Ode4
trMA.Ode4C13.mode = misc_en_UK_ware.Mode8
trMA.Ode4C13.melody = he.h.m8_en_UK_ware.ExOrousKataskiou.name
trMA.Ode4C13.incipit = "To the faithful who were…"
trMA.Ode4C13.text = "To the faithful who were taken from this life suddenly, at a time that they expected not, in the midst of joy or sorrow, of prosperity or misfortune: give rest, our Saviour, to them all."

trMA.Ode4C14.ode = misc_en_UK_ware.Ode4
trMA.Ode4C14.mode = misc_en_UK_ware.Mode8
trMA.Ode4C14.melody = he.h.m8_en_UK_ware.ExOrousKataskiou.name
trMA.Ode4C14.incipit = "To those destroyed by the…"
trMA.Ode4C14.text = "To those destroyed by the cold, killed by falling from their horse; overwhelmed by hail, snow or thunder-storms, crushed by stones or suffocated in the earth, give rest, O Christ our Saviour."

trMA.Ode4C1Tr.ode = misc_en_UK_ware.Ode4
trMA.Ode4C1Tr.mode = misc_en_UK_ware.Mode8
trMA.Ode4C1Tr.melody = he.h.m8_en_UK_ware.ExOrousKataskiou.name
trMA.Ode4C1Tr.incipit = "Strange it is that the…"
trMA.Ode4C1Tr.text = "Strange it is that the Godhead should be One and Three, fully present in each single Person without division: for Father, Son and Holy Spirit are worshipped as one God."

trMA.Ode4C1T.ode = misc_en_UK_ware.Ode4
trMA.Ode4C1T.mode = misc_en_UK_ware.Mode8
trMA.Ode4C1T.melody = he.h.m8_en_UK_ware.ExOrousKataskiou.name
trMA.Ode4C1T.incipit = "O Mother of the Lord…"
trMA.Ode4C1T.text = "O Mother of the Lord, we are buffeted by the storm of sin: pilot us by thy prayers, deliver us from every danger, and guide us to the haven of salvation."

trMA.Ode4K.ode = misc_en_UK_ware.Ode4
trMA.Ode4K.mode = misc_en_UK_ware.Mode8
trMA.Ode4K.melody = misc_en_UK_ware.HeirmosNotMetered
trMA.Ode4K.incipit = he.h.m8_en_UK_ware.ExOrousKataskiou.incipit
trMA.Ode4K.text = he.h.m8_en_UK_ware.ExOrousKataskiou.text

trMA.Ode5C1H.ode = misc_en_UK_ware.Ode5
trMA.Ode5C1H.mode = misc_en_UK_ware.Mode8
trMA.Ode5C1H.melody = misc_en_UK_ware.HeirmosNotMetered
trMA.Ode5C1H.incipit = he.h.m8_en_UK_ware.OrthrizeiToPnevmaMou.incipit
trMA.Ode5C1H.text = he.h.m8_en_UK_ware.OrthrizeiToPnevmaMou.text

trMA.Ode5C11.ode = misc_en_UK_ware.Ode5
trMA.Ode5C11.mode = misc_en_UK_ware.Mode8
trMA.Ode5C11.melody = he.h.m8_en_UK_ware.OrthrizeiToPnevmaMou.name
trMA.Ode5C11.incipit = "Celebrating today, O Lord, the…"
trMA.Ode5C11.text = "Celebrating today, O Lord, the memorial of all who from the beginning have died in the true faith, we cry fervently to Thee: Give rest to them all with Thy saints."

trMA.Ode5C12.ode = misc_en_UK_ware.Ode5
trMA.Ode5C12.mode = misc_en_UK_ware.Mode8
trMA.Ode5C12.melody = he.h.m8_en_UK_ware.OrthrizeiToPnevmaMou.name
trMA.Ode5C12.incipit = "Those whom Thou hast taken…"
trMA.Ode5C12.text = "Those whom Thou hast taken from every generation, Orthodox kings, rulers and monks, do Thou in Thy compassion deliver from eternal punishment."

trMA.Ode5C13.ode = misc_en_UK_ware.Ode5
trMA.Ode5C13.mode = misc_en_UK_ware.Mode8
trMA.Ode5C13.melody = he.h.m8_en_UK_ware.OrthrizeiToPnevmaMou.name
trMA.Ode5C13.incipit = "Thou knowest what is best…"
trMA.Ode5C13.text = "Thou knowest what is best for all the creatures Thou hast formed: deliver from every torment, O Lord our God, those whom Thou hast permitted to die unexpectedly, through some sudden mishap."

trMA.Ode5C14.ode = misc_en_UK_ware.Ode5
trMA.Ode5C14.mode = misc_en_UK_ware.Mode8
trMA.Ode5C14.melody = he.h.m8_en_UK_ware.OrthrizeiToPnevmaMou.name
trMA.Ode5C14.incipit = "From the ever-burning fire, from…"
trMA.Ode5C14.text = "From the ever-burning fire, from the darkness without light, from the gnashing of teeth and the worm that torments without ceasing, from every punishment deliver, O our Saviour, all who have died in faith.8"

trMA.Ode5C1Tr.ode = misc_en_UK_ware.Ode5
trMA.Ode5C1Tr.mode = misc_en_UK_ware.Mode8
trMA.Ode5C1Tr.melody = he.h.m8_en_UK_ware.OrthrizeiToPnevmaMou.name
trMA.Ode5C1Tr.incipit = "One in throne, without beginning…"
trMA.Ode5C1Tr.text = "One in throne, without beginning, threefold Unity, single in Nature yet distinct in Persons, unite us in the one will of Thy commandments."

trMA.Ode5C1T.ode = misc_en_UK_ware.Ode5
trMA.Ode5C1T.mode = misc_en_UK_ware.Mode8
trMA.Ode5C1T.melody = he.h.m8_en_UK_ware.OrthrizeiToPnevmaMou.name
trMA.Ode5C1T.incipit = "Thou wast made higher in…"
trMA.Ode5C1T.text = "Thou wast made higher in honour than the fiery seraphim, O pure Virgin, for thou hast borne Him whom they dare not approach, Jesus the Saviour, who by taking flesh from thee has made our earthly nature godlike."

trMA.Ode5K.ode = misc_en_UK_ware.Ode5
trMA.Ode5K.mode = misc_en_UK_ware.Mode8
trMA.Ode5K.melody = misc_en_UK_ware.HeirmosNotMetered
trMA.Ode5K.incipit = he.h.m8_en_UK_ware.OrthrizeiToPnevmaMou.incipit
trMA.Ode5K.text = he.h.m8_en_UK_ware.OrthrizeiToPnevmaMou.text

trMA.Ode6C1H.ode = misc_en_UK_ware.Ode6
trMA.Ode6C1H.mode = misc_en_UK_ware.Mode8
trMA.Ode6C1H.melody = misc_en_UK_ware.HeirmosNotMetered
trMA.Ode6C1H.incipit = he.h.m8_en_UK_ware.SynechomenonDexaiMe.incipit
trMA.Ode6C1H.text = he.h.m8_en_UK_ware.SynechomenonDexaiMe.text

trMA.Ode6C11.ode = misc_en_UK_ware.Ode6
trMA.Ode6C11.mode = misc_en_UK_ware.Mode8
trMA.Ode6C11.melody = he.h.m8_en_UK_ware.SynechomenonDexaiMe.name
trMA.Ode6C11.incipit = "Author of Life, by Thy…"
trMA.Ode6C11.text = "Author of Life, by Thy Passion Thou hast loosed the pains of death: give rest, our God, to Thy servants who have fallen asleep from the beginning of time."

trMA.Ode6C12.ode = misc_en_UK_ware.Ode6
trMA.Ode6C12.mode = misc_en_UK_ware.Mode8
trMA.Ode6C12.melody = he.h.m8_en_UK_ware.SynechomenonDexaiMe.name
trMA.Ode6C12.incipit = "To those whom in Thy…"
trMA.Ode6C12.text = "To those whom in Thy ineffable providence Thou hast permitted to be killed by drugs or poison, or through choking on bones, give rest, O Lord, with Thy saints."

trMA.Ode6C13.ode = misc_en_UK_ware.Ode6
trMA.Ode6C13.mode = misc_en_UK_ware.Mode8
trMA.Ode6C13.melody = he.h.m8_en_UK_ware.SynechomenonDexaiMe.name
trMA.Ode6C13.incipit = "When Thou shalt come as…"
trMA.Ode6C13.text = "When Thou shalt come as judge and all things stand naked and laid bare before Thy face, then in Thy mercy spare, O God, those who worshipped Thee in faith."

trMA.Ode6C14.ode = misc_en_UK_ware.Ode6
trMA.Ode6C14.mode = misc_en_UK_ware.Mode8
trMA.Ode6C14.melody = he.h.m8_en_UK_ware.SynechomenonDexaiMe.name
trMA.Ode6C14.incipit = "When Thine Archangel shall sound…"
trMA.Ode6C14.text = "When Thine Archangel shall sound the last trumpet, awakening all to the resurrection of life, then, O Christ, give rest to Thy servants."

trMA.Ode6C15.ode = misc_en_UK_ware.Ode6
trMA.Ode6C15.mode = misc_en_UK_ware.Mode8
trMA.Ode6C15.melody = he.h.m8_en_UK_ware.SynechomenonDexaiMe.name
trMA.Ode6C15.incipit = "Be merciful, O God, to…"
trMA.Ode6C15.text = "Be merciful, O God, to all the faithful from every nation of man­ kind, whom Thou hast taken since the beginning of time, and count them worthy to glorify Thee with Thy servants for ever."

trMA.Ode6C1Tr.ode = misc_en_UK_ware.Ode6
trMA.Ode6C1Tr.mode = misc_en_UK_ware.Mode8
trMA.Ode6C1Tr.melody = he.h.m8_en_UK_ware.SynechomenonDexaiMe.name
trMA.Ode6C1Tr.incipit = "Thrice-holy Godhead, one in throne…"
trMA.Ode6C1Tr.text = "Thrice-holy Godhead, one in throne, Father, Son and Spirit, Thou art my God, holding all in unity by Thine almighty power."

trMA.Ode6C1T.ode = misc_en_UK_ware.Ode6
trMA.Ode6C1T.mode = misc_en_UK_ware.Mode8
trMA.Ode6C1T.melody = he.h.m8_en_UK_ware.SynechomenonDexaiMe.name
trMA.Ode6C1T.incipit = "Leap for joy, Jesse the…"
trMA.Ode6C1T.text = "Leap for joy, Jesse the forefather; from thy root has sprung the Flower of Life that saves the world, Christ our God, born from the pure Maiden."

trMA.Ode6K.ode = misc_en_UK_ware.Ode6
trMA.Ode6K.mode = misc_en_UK_ware.Mode8
trMA.Ode6K.melody = misc_en_UK_ware.HeirmosNotMetered
trMA.Ode6K.incipit = he.h.m8_en_UK_ware.SynechomenonDexaiMe.incipit
trMA.Ode6K.text = he.h.m8_en_UK_ware.SynechomenonDexaiMe.text

trMA.Kontakion1.mode = misc_en_UK_ware.Mode8
trMA.Kontakion1.melody = ""
trMA.Kontakion1.incipit = "With the saints give rest…"
trMA.Kontakion1.text = "With the saints give rest, O Christ, to the souls of Thy servants, where there is no pain, no sorrow, no sighing, but life without end."

trMA.Oikos1.text = "Thou only art immortal, who hast created and fashioned man: but we are mortal, formed from the earth, and to this same earth shall we return, as Thou hast commanded when Thou madest me, saying: 'Dust thou art, and unto dust shalt thou return.' There shall all we mortals go, and for our funeral dirge we sing: Alleluia, Alleluia, Alleluia."

