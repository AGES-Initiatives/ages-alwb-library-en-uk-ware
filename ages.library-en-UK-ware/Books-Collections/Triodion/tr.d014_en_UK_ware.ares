trMA.Ode7C1H.text = he.h.m8_en_UK_ware.OEnArchiTinGin.text

trMA.Ode7C11.ode = misc_en_UK_ware.Ode7
trMA.Ode7C11.mode = misc_en_UK_ware.Mode8
trMA.Ode7C11.melody = he.h.m8_en_UK_ware.OEnArchiTinGin.name
trMA.Ode7C11.incipit = "Celebrating the memory of those…"
trMA.Ode7C11.text = "Celebrating the memory of those who from the beginning have died in the true faith, we cry aloud: Blessed art Thou unto all ages,"

trMA.Ode7C12.ode = misc_en_UK_ware.Ode7
trMA.Ode7C12.mode = misc_en_UK_ware.Mode8
trMA.Ode7C12.melody = he.h.m8_en_UK_ware.OEnArchiTinGin.name
trMA.Ode7C12.incipit = "Give rest, O God, to…"
trMA.Ode7C12.text = "Give rest, O God, to the faithful that have fallen asleep, who perished suddenly, struck by some weapon of iron, wood or stone."

trMA.Ode7C13.ode = misc_en_UK_ware.Ode7
trMA.Ode7C13.mode = misc_en_UK_ware.Mode8
trMA.Ode7C13.melody = he.h.m8_en_UK_ware.OEnArchiTinGin.name
trMA.Ode7C13.incipit = "At Thy dread coming, O…"
trMA.Ode7C13.text = "At Thy dread coming, O merciful Christ, place with the sheep at Thy right hand all those who in this life served Thee in the true Orthodox faith, and have now departed to Thee."

trMA.Ode7C14.ode = misc_en_UK_ware.Ode7
trMA.Ode7C14.mode = misc_en_UK_ware.Mode8
trMA.Ode7C14.melody = he.h.m8_en_UK_ware.OEnArchiTinGin.name
trMA.Ode7C14.incipit = "Grant to Thy servants, O…"
trMA.Ode7C14.text = "Grant to Thy servants, O Christ, a place in the choir of Thine elect, that they may cry aloud to Thee: Blessed art Thou unto all ages, O Lord God of our fathers."

trMA.Ode7C15.ode = misc_en_UK_ware.Ode7
trMA.Ode7C15.mode = misc_en_UK_ware.Mode8
trMA.Ode7C15.melody = he.h.m8_en_UK_ware.OEnArchiTinGin.name
trMA.Ode7C15.incipit = "Thou hast fashioned our clay…"
trMA.Ode7C15.text = "Thou hast fashioned our clay from the dust of the earth, O merciful Saviour, and quickened it by the Spirit: give rest, O God, to those whom Thou hast taken, in the life that grows not old."

trMA.Ode7C1Tr.ode = misc_en_UK_ware.Ode7
trMA.Ode7C1Tr.mode = misc_en_UK_ware.Mode8
trMA.Ode7C1Tr.melody = he.h.m8_en_UK_ware.OEnArchiTinGin.name
trMA.Ode7C1Tr.incipit = "Let us praise the Godhead…"
trMA.Ode7C1Tr.text = "Let us praise the Godhead, one in Nature but threefold in Persons, Father, Son and Holy Spirit, three Suns with but one single light."

trMA.Ode7C1T.ode = misc_en_UK_ware.Ode7
trMA.Ode7C1T.mode = misc_en_UK_ware.Mode8
trMA.Ode7C1T.melody = he.h.m8_en_UK_ware.OEnArchiTinGin.name
trMA.Ode7C1T.incipit = "With the words of David…"
trMA.Ode7C1T.text = "With the words of David we sing to thee, O Virgin, calling thee the mountain of God: in thee the Word dwelt according to the flesh, and in thee He has spiritually made our nature godlike."

trMA.Ode7K.ode = misc_en_UK_ware.Ode7
trMA.Ode7K.mode = misc_en_UK_ware.Mode8
trMA.Ode7K.melody = misc_en_UK_ware.HeirmosNotMetered
trMA.Ode7K.incipit = he.h.m8_en_UK_ware.OEnArchiTinGin.incipit
trMA.Ode7K.text = he.h.m8_en_UK_ware.OEnArchiTinGin.text

trMA.Ode8C1H.ode = misc_en_UK_ware.Ode8
trMA.Ode8C1H.mode = misc_en_UK_ware.Mode8
trMA.Ode8C1H.melody = misc_en_UK_ware.HeirmosNotMetered
trMA.Ode8C1H.incipit = he.h.m8_en_UK_ware.TonEnOreiAgio.incipit
trMA.Ode8C1H.text = he.h.m8_en_UK_ware.TonEnOreiAgio.text

trMA.Ode8C11.ode = misc_en_UK_ware.Ode8
trMA.Ode8C11.mode = misc_en_UK_ware.Mode8
trMA.Ode8C11.melody = he.h.m8_en_UK_ware.TonEnOreiAgio.name
trMA.Ode8C11.incipit = "Thou hast destroyed the shadow…"
trMA.Ode8C11.text = "Thou hast destroyed the shadow of death and shone forth from the tomb as the rising sun: make all those from every age who died in faith children of Thy Resurrection, O Lord of glory."

trMA.Ode8C12.ode = misc_en_UK_ware.Ode8
trMA.Ode8C12.mode = misc_en_UK_ware.Mode8
trMA.Ode8C12.melody = he.h.m8_en_UK_ware.TonEnOreiAgio.name
trMA.Ode8C12.incipit = "Thou who knowest things obscure…"
trMA.Ode8C12.text = "Thou who knowest things obscure and hidden, when Thou shalt come to reveal the works of darkness and the counsels of our hearts, then show mercy to all who have fallen asleep in faith, and do not require from them what is due."

trMA.Ode8C13.ode = misc_en_UK_ware.Ode8
trMA.Ode8C13.mode = misc_en_UK_ware.Mode8
trMA.Ode8C13.melody = he.h.m8_en_UK_ware.TonEnOreiAgio.name
trMA.Ode8C13.incipit = "When Thou shalt come to…"
trMA.Ode8C13.text = "When Thou shalt come to sit upon the throne and shalt summon with the trumpet all men from the ends of the earth, commanding them to stand before Thee for judgement, then spare us all, O Christ, in Thy mercy."

trMA.Ode8C14.ode = misc_en_UK_ware.Ode8
trMA.Ode8C14.mode = misc_en_UK_ware.Mode8
trMA.Ode8C14.melody = he.h.m8_en_UK_ware.TonEnOreiAgio.name
trMA.Ode8C14.incipit = "Those of the faithful who…"
trMA.Ode8C14.text = "Those of the faithful who died suddenly in some accident, as they cried out violently or ran swiftly, were struck on the face or trampled underfoot, do Thou forgive, O Lord of glory, unto all the ages."

trMA.Ode8C1Tr.ode = misc_en_UK_ware.Ode8
trMA.Ode8C1Tr.mode = misc_en_UK_ware.Mode8
trMA.Ode8C1Tr.melody = he.h.m8_en_UK_ware.TonEnOreiAgio.name
trMA.Ode8C1Tr.incipit = "As Unity in Essence I…"
trMA.Ode8C1Tr.text = "As Unity in Essence I sing Thy praises; as Trinity in Persons I venerate Thee, Father, Son and All-Holy Spirit. The power of Thy Kingdom that has no beginning I glorify unto all ages."

trMA.Ode8C1T.ode = misc_en_UK_ware.Ode8
trMA.Ode8C1T.mode = misc_en_UK_ware.Mode8
trMA.Ode8C1T.melody = he.h.m8_en_UK_ware.TonEnOreiAgio.name
trMA.Ode8C1T.incipit = "Thou art revealed, O Theotokos…"
trMA.Ode8C1T.text = "Thou art revealed, O Theotokos Virgin, as a sealed fountain of living water. For without man thou hast given birth to the Lord, and thou makest the faithful to drink from the waters of immortality unto all ages."

trMA.Ode8K.ode = misc_en_UK_ware.Ode8
trMA.Ode8K.mode = misc_en_UK_ware.Mode8
trMA.Ode8K.melody = misc_en_UK_ware.HeirmosNotMetered
trMA.Ode8K.incipit = he.h.m8_en_UK_ware.TonEnOreiAgio.incipit
trMA.Ode8K.text = he.h.m8_en_UK_ware.TonEnOreiAgio.text

trMA.Ode9C1H.ode = misc_en_UK_ware.Ode9
trMA.Ode9C1H.mode = misc_en_UK_ware.Mode8
trMA.Ode9C1H.melody = misc_en_UK_ware.HeirmosNotMetered
trMA.Ode9C1H.incipit = he.h.m8_en_UK_ware.TonProdilothenta.incipit
trMA.Ode9C1H.text = he.h.m8_en_UK_ware.TonProdilothenta.text

trMA.Ode9C11.ode = misc_en_UK_ware.Ode9
trMA.Ode9C11.mode = misc_en_UK_ware.Mode8
trMA.Ode9C11.melody = he.h.m8_en_UK_ware.TonProdilothenta.name
trMA.Ode9C11.incipit = "Where Thy saints dwell in…"
trMA.Ode9C11.text = "Where Thy saints dwell in joy, O Lord, grant that all from every age who fell asleep in faith and hope may also rejoice and be glad."

trMA.Ode9C12.ode = misc_en_UK_ware.Ode9
trMA.Ode9C12.mode = misc_en_UK_ware.Mode8
trMA.Ode9C12.melody = he.h.m8_en_UK_ware.TonProdilothenta.name
trMA.Ode9C12.incipit = "Give rest, O Christ, to…"
trMA.Ode9C12.text = "Give rest, O Christ, to all the faithful destroyed by the wrath of God: struck down by deadly thunderbolts from heaven, swallowed by a cleft in the earth, or drowned in the sea."

trMA.Ode9C13.ode = misc_en_UK_ware.Ode9
trMA.Ode9C13.mode = misc_en_UK_ware.Mode8
trMA.Ode9C13.melody = he.h.m8_en_UK_ware.TonProdilothenta.name
trMA.Ode9C13.incipit = "Give rest, O God, to…"
trMA.Ode9C13.text = "Give rest, O God, to all the faithful Thou hast taken from every age in life: old and young, children and new-born babies, male and female."

trMA.Ode9C14.ode = misc_en_UK_ware.Ode9
trMA.Ode9C14.mode = misc_en_UK_ware.Mode8
trMA.Ode9C14.melody = he.h.m8_en_UK_ware.TonProdilothenta.name
trMA.Ode9C14.incipit = "Give rest to all that…"
trMA.Ode9C14.text = "Give rest to all that worshipped Thee in faith: killed by poisonous bites, swallowed by serpents, trampled underfoot by horses, strangled or hanged by their neighbours."

trMA.Ode9C15.ode = misc_en_UK_ware.Ode9
trMA.Ode9C15.mode = misc_en_UK_ware.Mode8
trMA.Ode9C15.melody = he.h.m8_en_UK_ware.TonProdilothenta.name
trMA.Ode9C15.incipit = "We remember by name each…"
trMA.Ode9C15.text = "We remember by name each of the faithful who died in every generation from the beginning of time : grant them at Thy coming to stand before Thee uncondemned."

trMA.Ode9C1Tr.ode = misc_en_UK_ware.Ode9
trMA.Ode9C1Tr.mode = misc_en_UK_ware.Mode8
trMA.Ode9C1Tr.melody = he.h.m8_en_UK_ware.TonProdilothenta.name
trMA.Ode9C1Tr.incipit = "O God, one in three…"
trMA.Ode9C1Tr.text = "O God, one in three, glory be to Thee without ceasing. Each is God, yet Father, Son and Spirit are one in Nature, though threefold and distinct in the splendour of their Persons."

trMA.Ode9C1T.ode = misc_en_UK_ware.Ode9
trMA.Ode9C1T.mode = misc_en_UK_ware.Mode8
trMA.Ode9C1T.melody = he.h.m8_en_UK_ware.TonProdilothenta.name
trMA.Ode9C1T.incipit = "Beyond our understanding is thy…"
trMA.Ode9C1T.text = "Beyond our understanding is thy childbearing, O undefiled Virgin. For thou givest birth to Him who was before all things, and ineffably thou feedest with thy milk Him that feeds the world; thou holdest Him that upholds the universe, Christ our only Redeemer."

trMA.Ode9K.ode = misc_en_UK_ware.Ode9
trMA.Ode9K.mode = misc_en_UK_ware.Mode8
trMA.Ode9K.melody = misc_en_UK_ware.HeirmosNotMetered
trMA.Ode9K.incipit = he.h.m8_en_UK_ware.TonProdilothenta.incipit
trMA.Ode9K.text = he.h.m8_en_UK_ware.TonProdilothenta.text

trMA.Exaposteilarion1.mode = misc_en_UK_ware.Mode3
trMA.Exaposteilarion1.melody = he.a.m3_en_UK_ware.OOuranonToisAstrois.name
trMA.Exaposteilarion1.incipit = "O Thou who hast authority…"
trMA.Exaposteilarion1.text = "O Thou who hast authority as God over both the living and the dead, give rest to Thy servants in the dwelling-place of the elect, for though they have sinned, O Saviour, yet they did not tum away from Thee."

trMA.Exaposteilarion2.mode = misc_en_UK_ware.Mode2
trMA.Exaposteilarion2.melody = he.a.m2_en_UK_ware.GynaikesAkoutisthite.name
trMA.Exaposteilarion2.incipit = "Give rest to Thy servants…"
trMA.Exaposteilarion2.text = "Give rest to Thy servants, O Lord, in the land of the living from which pain, sorrow and sighing have fled away. In Thy love for man­ kind be merciful to the sins that they committed in this life : for Thou alone art sinless and merciful, O Master of the dead and the living."

trMA.ExapTheotokion.mode = misc_en_UK_ware.Mode2
trMA.ExapTheotokion.melody = he.a.m2_en_UK_ware.GynaikesAkoutisthite.name
trMA.ExapTheotokion.incipit = "O Mary, Bride of God…"
trMA.ExapTheotokion.text = "O Mary, Bride of God, pray to Christ without ceasing on behalf of us thy servants, that with the prophets inspired by God, and the companies of the martyrs, of hierarchs, holy monks and all the righteous, we may become fellow-heirs of the Kingdom of Heaven."

trMA.Lauds1.mode = misc_en_UK_ware.Mode8
trMA.Lauds1.melody = he.a.m8_en_UK_ware.OEnEdemParadeisos.name
trMA.Lauds1.incipit = "Come, brethren, before the end…"
trMA.Lauds1.text = "Come, brethren, before the end, and let us all look upon our clay, upon the infirmity and meanness of our nature. Let us behold our end, and the organs of the vessel of our flesh. Let us see that man is dust, food for worms, and corruption; that our bones grow dry, and have no breath of life within them. Let us gaze on the tombs. Where is man's glory? Where his outward beauty? Where is the eloquent tongue? Where the noble brow, and where the eye? All is dust and shadow. Therefore, Saviour, spare us all."

trMA.Lauds2.mode = misc_en_UK_ware.Mode8
trMA.Lauds2.melody = he.a.m8_en_UK_ware.OEnEdemParadeisos.name
trMA.Lauds2.incipit = "Why does man deceive himself…"
trMA.Lauds2.text = "Why does man deceive himself and boast? Why does he trouble himself in vain? For he is earth, and soon to the earth he will return. Why does the dust not reflect that it is formed from clay, and cast out as rottenness and corruption? Yet though we men are clay, why do we cling so closely to the earth? For if we are Christ's kindred, should we not run to Him, leaving all this mortal and fleeting life, and seeking the life incorruptible, which is Christ Himself, the illumination of our souls?"

trMA.Lauds3.mode = misc_en_UK_ware.Mode8
trMA.Lauds3.melody = he.a.m8_en_UK_ware.OEnEdemParadeisos.name
trMA.Lauds3.incipit = "Thou hast formed Adam with…"
trMA.Lauds3.text = "Thou hast formed Adam with Thine hand, O Saviour, and set him on the border between incorruption and mortality; Thou hast made him share in life through grace, freeing him from corruption and translating him to the life that he enjoyed at first. Give rest, O Master, to Thy servants Thou hast taken from us; may they dwell with the righteous in the choir of Thine elect; write their names in the book of life; raise them with the sound of the Archangel's trump, and count them worthy of Thy heavenly Kingdom."

trMA.Lauds4.mode = misc_en_UK_ware.Mode8
trMA.Lauds4.melody = he.a.m8_en_UK_ware.OEnEdemParadeisos.name
trMA.Lauds4.incipit = "and destroying the power of…"
trMA.Lauds4.text = "and destroying the power of hell. Be of good courage, all ye dead, for death is slain and hell despoiled; the crucified and risen Christ is King. He has given incorruption to our flesh; He raises us and grants us resurrection, and He counts worthy of His joy and glory all who, with a faith that wavers not, have trusted fervently in Him."

trMA.LaudsGlory.mode = misc_en_UK_ware.Mode2
trMA.LaudsGlory.melody = ""
trMA.LaudsGlory.incipit = "As a flower withers and…"
trMA.LaudsGlory.text = "As a flower withers and a dream passes away, so is each man's flesh dissolved at death. But at the sound of the trump all the dead as in an earthquake shall rise up again to meet Thee, Christ our God. Then, Master, grant to all Thy servants whom Thou hast taken from us, that they may dwell for ever in the tabernacles of Thy saints." //oc.m2.d7_en_UK_ware.ocVE.Aposticha4.text

trMA.LaudsTheotokion.mode = misc_en_UK_ware.Mode2
trMA.LaudsTheotokion.melody = ""
trMA.LaudsTheotokion.incipit = "Hail Mary Theotokos, temple that…"
trMA.LaudsTheotokion.text = "Hail Mary Theotokos, temple that shall never be destroyed, temple of holiness, as the Prophet cries: 'Holy is Thy temple, wonderful in righteousness.'" //oc.m2.d7_en_UK_ware.octrMA.LaudsTheotokion.text

trMA.AposGlory.mode = misc_en_UK_ware.Mode6
trMA.AposGlory.melody = ""
trMA.AposGlory.incipit = "In days of old in…"
trMA.AposGlory.text = "In days of old in Eden the eating of the fruit brought grief to Adam, through the poison of the serpent; for in this way death entered, devouring the whole race of man. But the Master by His coming has destroyed the dragon and bestowed upon us rest. To Him, therefore, let us cry aloud: Spare, O Saviour, those whom Thou hast taken, and give them rest with Thine elect." //oc.m6.d7_en_UK_ware.ocVE.Aposticha3.text

trMA.AposTheotokion.mode = misc_en_UK_ware.Mode6
trMA.AposTheotokion.melody = ""
trMA.AposTheotokion.incipit = "Thou art our God, that…"
trMA.AposTheotokion.text = "Thou art our God, that in wisdom createst and fillest all things. Thou hast sent prophets, O Christ, to foretell Thine advent, and apostles to proclaim Thy majesty; the first prophesied of Thy coming, and the second illumined the nations by baptism, while the martyrs through their sufferings received what they desired. With Thy Mother they all intercede before Thee: Give rest, O Lord, to the souls that Thou hast taken; and, O my Deliverer and my God, who hast endured the Cross for me that am condemned, count us worthy of Thy Kingdom." //oc.m6.d7_en_UK_ware.ocMA.LaudsTheotokion.text

