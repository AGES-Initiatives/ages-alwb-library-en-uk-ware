meMA.Ode8C21.greekmelody = he.h.m2_en_UK_ware.heHE.EleftheraMenIKtisis.greekname
meMA.Ode8C21.melody = he.h.m2_en_UK_ware.heHE.EleftheraMenIKtisis.name
meMA.Ode8C21.incipit = "The three godly Children who…"
meMA.Ode8C21.text = "The three godly Children who were sprinkled with dew in the fire Plainly prefigured how the Most High God, Who shines with the bright rays of a threefold holiness, Should mingle with mortal men, to their great blessing, Consuming all deadly error in the fire of dew."
meMA.Ode8C211.text = "The three godly Children who were sprinkled with dew in the fire"
meMA.Ode8C212.text = "Plainly prefigured how the Most High God,"
meMA.Ode8C213.text = "Who shines with the bright rays of a threefold holiness,"
meMA.Ode8C214.text = "Should mingle with mortal men, to their great blessing,"
meMA.Ode8C215.text = "Consuming all deadly error in the fire of dew."

meMA.Ode8C22.ode = miscellanea_en_UK_ware.misc.Ode8
meMA.Ode8C22.mode = miscellanea_en_UK_ware.misc.Mode2
meMA.Ode8C22.melody = he.h.m2_en_UK_ware.heHE.EleftheraMenIKtisis.name
meMA.Ode8C22.incipit = "Let the whole earthly creation…"
meMA.Ode8C22.text = "Let the whole earthly creation clothe itself in white, For this day it is raised up from its fall from heaven. The Word who preserves all things Has cleansed it in the flowing waters: Washed and resplendent, it has escaped from its former sins."
meMA.Ode8C221.text = "Let the whole earthly creation clothe itself in white,"
meMA.Ode8C222.text = "For this day it is raised up from its fall from heaven."
meMA.Ode8C223.text = "The Word who preserves all things"
meMA.Ode8C224.text = "Has cleansed it in the flowing waters:"
meMA.Ode8C225.text = "Washed and resplendent, it has escaped from its former sins."

meMA.Ode9C1M01.incipit = "…"
meMA.Ode9C1M01.text = "Magnify, O my soul, her who is greater in honour than the hosts on high."
meMA.Ode9C1M02.incipit = "…"
meMA.Ode9C1M02.text = "Magnify, O my soul, Him who is come to be baptized in Jordan."
meMA.Ode9C1M03.incipit = "…"
meMA.Ode9C1M03.text = "Magnify, O my soul, Him who receives baptism from the Forerunner."
meMA.Ode9C1M04.incipit = "…"
meMA.Ode9C1M04.text = "Magnify, O my soul, Him to whom the voice of the Father bore witness."
meMA.Ode9C1M05.incipit = "…"
meMA.Ode9C1M05.text = "Magnify, O my soul, one of the Trinity who bowed His head and received baptism."
meMA.Ode9C1M06.incipit = "…"
meMA.Ode9C1M06.text = "‘O Prophet, come to Me: stretch out thine hand and baptize Me swiftly.’"
meMA.Ode9C1M07.incipit = "…"
meMA.Ode9C1M07.text = "‘O Prophet, suffer it to be so now, and baptize me as | wish: for l have come to fulfil all righteousness.’"

meMA.Ode9C2M01.incipit = "…"
meMA.Ode9C2M01.text = "Today the Master bows His head beneath the hand of the Forerunner."
meMA.Ode9C2M02.incipit = "…"
meMA.Ode9C2M02.text = "Today John baptizes the Master in the streams of Jordan."
meMA.Ode9C2M03.incipit = "…"
meMA.Ode9C2M03.text = "Today the Master buries in the waters the sin of mortal man."
meMA.Ode9C2M04.incipit = "…"
meMA.Ode9C2M04.text = "Today the Master receives testimony from on high, that He is the beloved Son."
meMA.Ode9C2M05.incipit = "…"
meMA.Ode9C2M05.text = "Today the Master has come to sanctify the nature of the waters."
meMA.Ode9C2M06.incipit = "…"
meMA.Ode9C2M06.text = "Today the Master receives baptism at the hand of the Forerunner."
meMA.Ode9C2MTr.incipit = "…"
meMA.Ode9C2MTr.text = "Magnify, O my soul, the power of the undivided Godhead in three Persons."
meMA.Ode9C2MTh.incipit = "…"
meMA.Ode9C2MTh.text = "Magnify, O my soul, her that has delivered us from the curse."

