meMA.Ode9C21.ode = miscellanea_en_UK_ware.misc.Ode9
meMA.Ode9C21.mode = miscellanea_en_UK_ware.misc.Mode2
meMA.Ode9C21.greekmelody = he.h.m2_en_UK_ware.heHE.OTonYperNoun.greekname
meMA.Ode9C21.melody = he.h.m2_en_UK_ware.heHE.OTonYperNoun.name
meMA.Ode9C21.incipit = "That which was revealed to…"
meMA.Ode9C21.text = "That which was revealed to Moses in the bush We see accomplished here in strange manner. The Virgin bore Fire within her, yet was not consumed, When she gave birth to the Benefactor who brings us light, And the streams of Jordan suffered no harm when they received Him."
meMA.Ode9C211.text = "That which was revealed to Moses in the bush"
meMA.Ode9C212.text = "We see accomplished here in strange manner."
meMA.Ode9C213.text = "The Virgin bore Fire within her, yet was not consumed,"
meMA.Ode9C214.text = "When she gave birth to the Benefactor who brings us light,"
meMA.Ode9C215.text = "And the streams of Jordan suffered no harm when they received Him."

meMA.Ode9C22.ode = miscellanea_en_UK_ware.misc.Ode9
meMA.Ode9C22.mode = miscellanea_en_UK_ware.misc.Mode2
meMA.Ode9C22.melody = he.h.m2_en_UK_ware.heHE.OTonYperNoun.name
meMA.Ode9C22.incipit = "O King without beginning, through…"
meMA.Ode9C22.text = "O King without beginning, through the communion of the Spirit Thou dost anoint and make perfect the nature of mankind. Thou hast cleansed it in the undefiled streams, Putting to shame the arrogant force of darkness, And now Thou dost translate it unto endless life."
meMA.Ode9C221.text = "O King without beginning, through the communion of the Spirit"
meMA.Ode9C222.text = "Thou dost anoint and make perfect the nature of mankind."
meMA.Ode9C223.text = "Thou hast cleansed it in the undefiled streams,"
meMA.Ode9C224.text = "Putting to shame the arrogant force of darkness,"
meMA.Ode9C225.text = "And now Thou dost translate it unto endless life."

