{'id_' : 'tr|d048_en_UK_ware|trShortCommemoration1|text', 'text' : ''}
{'id_' : 'tr|d048_en_UK_ware|trInsert1', 'text' : ''}
{'id_' : 'tr|d048_en_UK_ware|trInsert2', 'text' : ''}
