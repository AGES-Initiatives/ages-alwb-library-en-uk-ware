{'id_' : 'tr|d052_en_UK_ware|trShortCommemoration1|text', 'text' : ''}
{'id_' : 'tr|d052_en_UK_ware|trInsert1', 'text' : ''}
{'id_' : 'tr|d052_en_UK_ware|trInsert2', 'text' : ''}
